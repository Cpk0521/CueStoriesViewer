{
    "Txt_Name": "HM_Sub_01_11_22",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "あ、マネージャー。\nちょっと意見聞きたいんだけどいい？",
            "voice": "audition/story_01/heroine_11/HM_Sub_01_11_22/hm_sub_01_11_22_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "昨日買い物行って、\n色違いのワンピ、二枚買ったんだよね。",
            "voice": "audition/story_01/heroine_11/HM_Sub_01_11_22/hm_sub_01_11_22_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "声優って声のお仕事がメインだけど、\nやっぱり印象は大事じゃない？",
            "voice": "audition/story_01/heroine_11/HM_Sub_01_11_22/hm_sub_01_11_22_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "だから、今度のオーディションでどっち着ようかなって、\n悩んでるわけ。",
            "voice": "audition/story_01/heroine_11/HM_Sub_01_11_22/hm_sub_01_11_22_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "で、見てよこれ。どっちがいいと思う？\nグリーンとブルー。",
            "voice": "audition/story_01/heroine_11/HM_Sub_01_11_22/hm_sub_01_11_22_005.mp3"
        }
    ]
}