{
    "Txt_Name": "HM_Cast_01_11_09_01",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "お疲れ様です、マネージャーさん。\nちょっとお話聞いてもらってもいいですか？",
            "voice": "audition/story_01/heroine_11/HM_Cast_01_11_09_01/hm_cast_01_11_09_01_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "お疲れ様。どうしたの？",
            "voice": ""
        },
        {
            "index": 3,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "今日、ランチの時、まほろと舞台の話で盛り上がったんです。",
            "voice": "audition/story_01/heroine_11/HM_Cast_01_11_09_01/hm_cast_01_11_09_01_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "わたしの好きな舞台のお話をしてたら、\nまほろもその舞台見てたらしくて……。",
            "voice": "audition/story_01/heroine_11/HM_Cast_01_11_09_01/hm_cast_01_11_09_01_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "しかも、同じ回を見てたんですって。",
            "voice": "audition/story_01/heroine_11/HM_Cast_01_11_09_01/hm_cast_01_11_09_01_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "へぇ、そんな偶然あるんだね。",
            "voice": ""
        },
        {
            "index": 7,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "はい。それで今度、その舞台の続編、\n一緒に見に行くことになったんです。",
            "voice": "audition/story_01/heroine_11/HM_Cast_01_11_09_01/hm_cast_01_11_09_01_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うふふ、今から待ち遠しい。",
            "voice": "audition/story_01/heroine_11/HM_Cast_01_11_09_01/hm_cast_01_11_09_01_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "そっか。楽しんできてね。",
            "voice": ""
        },
        {
            "index": 10,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "はい、また、お土産話も聞いてくださいね。\n約束ですよ。",
            "voice": "audition/story_01/heroine_11/HM_Cast_01_11_09_01/hm_cast_01_11_09_01_010.mp3"
        }
    ]
}