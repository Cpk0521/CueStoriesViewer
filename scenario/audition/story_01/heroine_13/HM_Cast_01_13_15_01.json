{
    "Txt_Name": "HM_Cast_01_13_15_01",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "凛音……、その服、どうしたの？\n随分と泥だらけだけど。",
            "voice": ""
        },
        {
            "index": 2,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "あ、これですか？　えへへ～。\nお昼に子どもたちと遊んでたら、転んじゃってー。",
            "voice": "audition/story_01/heroine_13/HM_Cast_01_13_15_01/hm_cast_01_13_15_01_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "ええっ。怪我は無かったの？",
            "voice": ""
        },
        {
            "index": 4,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "大丈夫です！　芝生の上だったので！\nレッスンもバッチリ出来ました！",
            "voice": "audition/story_01/heroine_13/HM_Cast_01_13_15_01/hm_cast_01_13_15_01_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "そう……。でも、気をつけて。\n怪我でもしたら、大変だから。",
            "voice": ""
        },
        {
            "index": 6,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "はい、明日は気をつけます！",
            "voice": "audition/story_01/heroine_13/HM_Cast_01_13_15_01/hm_cast_01_13_15_01_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "えっ、明日もまた遊ぶの？\nレッスンもあるよね？",
            "voice": ""
        },
        {
            "index": 8,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "はい！　だからレッスンまでみんなと遊ぶんです！\n約束もしてるので！",
            "voice": "audition/story_01/heroine_13/HM_Cast_01_13_15_01/hm_cast_01_13_15_01_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "じゃあマネージャーさん、お疲れ様でした！\n明日もよろしくお願いしまーす！",
            "voice": "audition/story_01/heroine_13/HM_Cast_01_13_15_01/hm_cast_01_13_15_01_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "お疲れ様……。元気だなあ。",
            "voice": ""
        }
    ]
}