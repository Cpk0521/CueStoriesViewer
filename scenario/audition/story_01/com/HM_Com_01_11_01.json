{
    "Txt_Name": "HM_Com_01_11_01",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "まほろ、何してるの？　",
            "voice": ""
        },
        {
            "index": 2,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "見てわかんない？　",
            "voice": "audition/story_01/com/HM_Com_01_11_01/hm_com_01_11_01_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "自撮り……？\nでも、どうしてここで？",
            "voice": ""
        },
        {
            "index": 4,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "事務所に届いてたファンレター、\nさっき受け取ったでしょ？",
            "voice": "audition/story_01/com/HM_Com_01_11_01/hm_com_01_11_01_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ほら、こうしてSNSに上げれば、ちゃんと受け取ったよって、\nメッセージにもなるわけ。",
            "voice": "audition/story_01/com/HM_Com_01_11_01/hm_com_01_11_01_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "なるほど。　",
            "voice": ""
        },
        {
            "index": 7,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ファンサービスは大事だしね。　",
            "voice": "audition/story_01/com/HM_Com_01_11_01/hm_com_01_11_01_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "そういうところに気が回るの、本当に偉いね。まほろは。　",
            "voice": ""
        },
        {
            "index": 9,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "何言ってんの。\n今いるファンを大事にするなんて当たり前じゃない？",
            "voice": "audition/story_01/com/HM_Com_01_11_01/hm_com_01_11_01_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "それじゃ、お疲れ様ー。　",
            "voice": "audition/story_01/com/HM_Com_01_11_01/hm_com_01_11_01_010.mp3"
        }
    ]
}