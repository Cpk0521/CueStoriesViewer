{
    "Txt_Name": "HM_Com_01_04_02",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "お疲れ様でしたー！\nあ、外って、まだ雨降ってますか？",
            "voice": "audition/story_01/com/HM_Com_01_04_02/hm_com_01_04_02_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "お疲れ様、ほのか。\nうん、まだ雨は続いてるよ。",
            "voice": ""
        },
        {
            "index": 3,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "そうですか……。結局、今日はずっと雨でしたね。　",
            "voice": "audition/story_01/com/HM_Com_01_04_02/hm_com_01_04_02_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "そうだね……。\nもしかして、今日も走ってきたの？",
            "voice": ""
        },
        {
            "index": 5,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "いやー、さすがにこの雨の中は走ってきませんよ。\n危ないですから。　",
            "voice": "audition/story_01/com/HM_Com_01_04_02/hm_com_01_04_02_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "今日はその分、帰って多めにトレーニングします！",
            "voice": "audition/story_01/com/HM_Com_01_04_02/hm_com_01_04_02_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "マネージャーも帰る時、足元気を付けてください。\n滑りやすいんで。",
            "voice": "audition/story_01/com/HM_Com_01_04_02/hm_com_01_04_02_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "それじゃ、お先に失礼します。　",
            "voice": "audition/story_01/com/HM_Com_01_04_02/hm_com_01_04_02_008.mp3"
        }
    ]
}