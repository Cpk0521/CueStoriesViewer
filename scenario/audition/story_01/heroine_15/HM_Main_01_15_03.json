{
    "Txt_Name": "HM_Main_01_15_03",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "……えっ。本当ですか！？",
            "voice": "audition/story_01/heroine_15/HM_Main_01_15_03/hm_main_01_15_03_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "うん。1次オーディション、通過おめでとう！",
            "voice": ""
        },
        {
            "index": 3,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "やった～！",
            "voice": "audition/story_01/heroine_15/HM_Main_01_15_03/hm_main_01_15_03_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "2次は少し間が空くけど、気を引き締めていこう。",
            "voice": ""
        },
        {
            "index": 5,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "はいっ！ ……次は2次かぁ。",
            "voice": "audition/story_01/heroine_15/HM_Main_01_15_03/hm_main_01_15_03_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "本当に、あっという間ですねー。",
            "voice": "audition/story_01/heroine_15/HM_Main_01_15_03/hm_main_01_15_03_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "そうだね。光陰矢の如し、って言うもんね。",
            "voice": ""
        },
        {
            "index": 8,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "それ、さとちんも言ってました！",
            "voice": "audition/story_01/heroine_15/HM_Main_01_15_03/hm_main_01_15_03_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "うん、これからも頑張ろうね。",
            "voice": ""
        },
        {
            "index": 10,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "はいっ！\n子どもたちからも元気をもらって、次も頑張ります！",
            "voice": "audition/story_01/heroine_15/HM_Main_01_15_03/hm_main_01_15_03_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "そっか、今日も遊ぶ予定なの？",
            "voice": ""
        },
        {
            "index": 12,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "はい！ \nあ、マネージャーさんも来ますか！？",
            "voice": "audition/story_01/heroine_15/HM_Main_01_15_03/hm_main_01_15_03_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "いや……仕事があるし、遠慮しておくよ。",
            "voice": ""
        },
        {
            "index": 14,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "えへへ、わかりました。\nでも、いつか一緒に来てくださいね！",
            "voice": "audition/story_01/heroine_15/HM_Main_01_15_03/hm_main_01_15_03_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "えへへ、今日は何をして遊ぼうかなあ～。\n昨日は隠れんぼをしたから……。",
            "voice": "audition/story_01/heroine_15/HM_Main_01_15_03/hm_main_01_15_03_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "いいですね！\nわたしもシャボン玉は大好きです！",
            "voice": "audition/story_01/heroine_15/HM_Main_01_15_03/hm_main_01_15_03_017.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "いいですね！\nそういえば、なんで「だるまさんがころんだ」って言うんでしょう？",
            "voice": "audition/story_01/heroine_15/HM_Main_01_15_03/hm_main_01_15_03_019.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "おままごと、すごく得意です！\nわたしのおじいちゃん役、すっごく上手って言われるんですよ～！",
            "voice": "audition/story_01/heroine_15/HM_Main_01_15_03/hm_main_01_15_03_021.mp3"
        }
    ]
}