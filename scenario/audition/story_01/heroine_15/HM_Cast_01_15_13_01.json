{
    "Txt_Name": "HM_Cast_01_15_13_01",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "じゃあ、お疲れ様です。\nマネージャー。",
            "voice": "audition/story_01/heroine_15/HM_Cast_01_15_13_01/hm_cast_01_15_13_01_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "うん。お疲れ様。\n外、雨降ってるみたいだから気をつけてね。",
            "voice": ""
        },
        {
            "index": 3,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "フッ、忠告感謝する。",
            "voice": "audition/story_01/heroine_15/HM_Cast_01_15_13_01/hm_cast_01_15_13_01_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "リエンヌ〜！　一緒に帰ろ！",
            "voice": "audition/story_01/heroine_15/HM_Cast_01_15_13_01/hm_cast_01_15_13_01_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "うん、帰ろ。\n……あれ？　凛音、傘は？",
            "voice": "audition/story_01/heroine_15/HM_Cast_01_15_13_01/hm_cast_01_15_13_01_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "あっ！　忘れちゃった！",
            "voice": "audition/story_01/heroine_15/HM_Cast_01_15_13_01/hm_cast_01_15_13_01_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "えっ？　天気予報、一緒に見てたのに。",
            "voice": "audition/story_01/heroine_15/HM_Cast_01_15_13_01/hm_cast_01_15_13_01_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "えへへ……お昼はすっごく晴れてたから。\n今日は走って帰るよ！",
            "voice": "audition/story_01/heroine_15/HM_Cast_01_15_13_01/hm_cast_01_15_13_01_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "その必要はないぞ。\n我輩の傘に入ればよい。",
            "voice": "audition/story_01/heroine_15/HM_Cast_01_15_13_01/hm_cast_01_15_13_01_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "えっ！　ありがとう！\nリエンヌは優しいなぁ。",
            "voice": "audition/story_01/heroine_15/HM_Cast_01_15_13_01/hm_cast_01_15_13_01_010.mp3"
        }
    ]
}