        {
            "index": 3,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "わたし、ちょっと自信あったんですけど、\nなるちゃんがすっごく上手で。",
            "voice": "audition/story_01/heroine_15/HM_Sub_01_15_18/hm_sub_01_15_18_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "……ハッ！　もしかしてなるちゃん、\nソフトクリーム屋さんだったのでは！",
            "voice": "audition/story_01/heroine_15/HM_Sub_01_15_18/hm_sub_01_15_18_004.mp3"
        }
