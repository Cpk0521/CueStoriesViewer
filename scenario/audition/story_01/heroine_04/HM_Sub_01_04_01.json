{
    "Txt_Name": "HM_Sub_01_04_01",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "お疲れ様です、マネージャー。\nオーディションの資料、色々とありがとうございました！",
            "voice": "audition/story_01/heroine_04/HM_Sub_01_04_01/hm_sub_01_04_01_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "こうやってオーディションに向けて準備するって、\nやっぱいいですよね。",
            "voice": "audition/story_01/heroine_04/HM_Sub_01_04_01/hm_sub_01_04_01_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "明確な目標があることもそうですけど、\n日頃の特訓の成果を発揮出来る、って考えただけで……。",
            "voice": "audition/story_01/heroine_04/HM_Sub_01_04_01/hm_sub_01_04_01_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "ふふっ、もっと頑張らなきゃって思います！",
            "voice": "audition/story_01/heroine_04/HM_Sub_01_04_01/hm_sub_01_04_01_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "帰ったらもっと資料を読み込みますね！\nお先に失礼します！",
            "voice": "audition/story_01/heroine_04/HM_Sub_01_04_01/hm_sub_01_04_01_005.mp3"
        }
    ]
}