{
    "Txt_Name": "HM_Cast_01_16_15_04",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "なるちゃん……、その袋、どうしたの？",
            "voice": "audition/story_01/heroine_16/HM_Cast_01_16_15_04/hm_cast_01_16_15_04_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "凛音さんからもらったお菓子。\nやっぱり1人じゃ食べきれなかった。",
            "voice": "audition/story_01/heroine_16/HM_Cast_01_16_15_04/hm_cast_01_16_15_04_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "え！　わたし、そんなにあげてたっけ？",
            "voice": "audition/story_01/heroine_16/HM_Cast_01_16_15_04/hm_cast_01_16_15_04_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "うん。",
            "voice": "audition/story_01/heroine_16/HM_Cast_01_16_15_04/hm_cast_01_16_15_04_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "そっかー。ごめんね。",
            "voice": "audition/story_01/heroine_16/HM_Cast_01_16_15_04/hm_cast_01_16_15_04_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "ううん、大丈夫。\nでも、もったいないから、凛音さんも食べて。",
            "voice": "audition/story_01/heroine_16/HM_Cast_01_16_15_04/hm_cast_01_16_15_04_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "うん、わかった！\n一緒に食べよう！　そのほうが、もっとおいしいよね！",
            "voice": "audition/story_01/heroine_16/HM_Cast_01_16_15_04/hm_cast_01_16_15_04_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "あ！　マネージャーさんもどうですか？",
            "voice": "audition/story_01/heroine_16/HM_Cast_01_16_15_04/hm_cast_01_16_15_04_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "えっ。いいの？",
            "voice": ""
        },
        {
            "index": 10,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "えへへ。幸せの、おすそ分けです！",
            "voice": "audition/story_01/heroine_16/HM_Cast_01_16_15_04/hm_cast_01_16_15_04_010.mp3"
        }
    ]
}