{
    "Txt_Name": "HM_Sub_01_05_03",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "この前、そろそろ顔出しとかないとなーって思って、\n久しぶりにウチの店に行ったんだけどさ。",
            "voice": "audition/story_01/heroine_05/HM_Sub_01_05_03/hm_sub_01_05_03_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "そしたら、ちょうど来てた常連さんが、\nめっちゃあたしのこと、応援してくれて。",
            "voice": "audition/story_01/heroine_05/HM_Sub_01_05_03/hm_sub_01_05_03_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "あたしがちょい役で出てる作品も見てくれてて、\nなんかうるっときちゃったよ。",
            "voice": "audition/story_01/heroine_05/HM_Sub_01_05_03/hm_sub_01_05_03_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "やっぱり応援されると、やる気も出るよね！",
            "voice": "audition/story_01/heroine_05/HM_Sub_01_05_03/hm_sub_01_05_03_004.mp3"
        }
    ]
}