{
    "Txt_Name": "HM_Main_01_05_02",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "マネージャー！　遅いよもう！",
            "voice": "audition/story_01/heroine_05/HM_Main_01_05_02/hm_main_01_05_02_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "オーディションはすぐそこに迫ってる！\nあたしたちには休んでる暇なんてないんだよ！",
            "voice": "audition/story_01/heroine_05/HM_Main_01_05_02/hm_main_01_05_02_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "やる気は十分って感じだね。",
            "voice": ""
        },
        {
            "index": 4,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "ずっとチャンスを待ってたからね！\nここでやらなかったら、いつやるんだって感じだよ！",
            "voice": "audition/story_01/heroine_05/HM_Main_01_05_02/hm_main_01_05_02_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "あぁ、資料見てるだけで燃えてくるなー！\n自由を求めて空へと飛び立つ少年少女！ ",
            "voice": "audition/story_01/heroine_05/HM_Main_01_05_02/hm_main_01_05_02_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "こんなの、あたしの為にあるオーディションと言っても、\n過言じゃないよ！",
            "voice": "audition/story_01/heroine_05/HM_Main_01_05_02/hm_main_01_05_02_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "悠希にとって、絶対に見逃せないオーディションだよね。",
            "voice": ""
        },
        {
            "index": 8,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "そうそう！\nマネージャーもわかってるじゃん！",
            "voice": "audition/story_01/heroine_05/HM_Main_01_05_02/hm_main_01_05_02_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "あたしは全力で突っ走るから、\nちゃんとついてきてよね！",
            "voice": "audition/story_01/heroine_05/HM_Main_01_05_02/hm_main_01_05_02_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "さっすがマネージャー！\nこのままノンストップでレッスンだよ！",
            "voice": "audition/story_01/heroine_05/HM_Main_01_05_02/hm_main_01_05_02_011.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "それくらいわかってるって！\nこういうのはまず、気持ちから入るのが大事なんだよ！",
            "voice": "audition/story_01/heroine_05/HM_Main_01_05_02/hm_main_01_05_02_013.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "へぇ、いいじゃん。つまり勝負ってことだね。\n言っとくけど、あたしは負けないよ！",
            "voice": "audition/story_01/heroine_05/HM_Main_01_05_02/hm_main_01_05_02_015.mp3"
        }
    ]
}