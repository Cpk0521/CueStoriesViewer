{
    "Txt_Name": "HM_Cast_01_06_05_02",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "ねぇねぇ！\n聞いてよ、マネージャー！",
            "voice": "audition/story_01/heroine_06/HM_Cast_01_06_05_02/hm_cast_01_06_05_02_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "お疲れ様、悠希。\nどうしたの？",
            "voice": ""
        },
        {
            "index": 3,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "実はさ、\n最近千紗に、ダイバリオンのこと色々聞かれるんだー。",
            "voice": "audition/story_01/heroine_06/HM_Cast_01_06_05_02/hm_cast_01_06_05_02_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "ダイバリオンっていうか、メカの良さについてだね。\n今まで興味無さそうだったのに、どうしたんだろ？",
            "voice": "audition/story_01/heroine_06/HM_Cast_01_06_05_02/hm_cast_01_06_05_02_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "なんか知りたくなったんじゃない？",
            "voice": ""
        },
        {
            "index": 6,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "なんかってなんだよー。\nでもまぁ、あたし的にはすごく嬉しいんだよね！",
            "voice": "audition/story_01/heroine_06/HM_Cast_01_06_05_02/hm_cast_01_06_05_02_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "千紗にも、やっと良さが伝わったかーって感慨深いよ！",
            "voice": "audition/story_01/heroine_06/HM_Cast_01_06_05_02/hm_cast_01_06_05_02_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "今度、ダイバリオンのディスク全巻貸して、\n資料集も見せてあげるんだ！",
            "voice": "audition/story_01/heroine_06/HM_Cast_01_06_05_02/hm_cast_01_06_05_02_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "そっか……出来れば程々にしてあげてね。",
            "voice": ""
        },
        {
            "index": 10,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "え、なんで？\nあたしは全身全霊で伝えるよ！　メカへの熱い想いを！",
            "voice": "audition/story_01/heroine_06/HM_Cast_01_06_05_02/hm_cast_01_06_05_02_010.mp3"
        }
    ]
}