    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "お疲れ様です。\nオーディションの資料、しっかり読み込んできました！",
            "voice": "audition/story_01/heroine_06/HM_Sub_01_06_01/hm_sub_01_06_01_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "世界観も凝ってて、すごく面白そうでした。",
            "voice": "audition/story_01/heroine_06/HM_Sub_01_06_01/hm_sub_01_06_01_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "でも、あのお話……。\n身に覚えがあるというか、なんというか……。",
            "voice": "audition/story_01/heroine_06/HM_Sub_01_06_01/hm_sub_01_06_01_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "……うーん、私、さらわれる女の子に縁があるのかしら。",
            "voice": "audition/story_01/heroine_06/HM_Sub_01_06_01/hm_sub_01_06_01_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "あはは、すみません。ちょっと思い出しちゃって。\nそれじゃあ、お先に失礼します。",
            "voice": "audition/story_01/heroine_06/HM_Sub_01_06_01/hm_sub_01_06_01_005.mp3"
        }
    ]
}