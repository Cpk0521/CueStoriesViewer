{
    "Txt_Name": "HM_Sub_01_06_09",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "この前、柚葉と洋服を見に行ったんです。\nそしたら……新作のワンピースがすっごくかわいくて！",
            "voice": "audition/story_01/heroine_06/HM_Sub_01_06_09/hm_sub_01_06_09_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "しばらくお店の前をうろうろしちゃうくらい素敵だったんです。\nでも、結局、買わなかったんですよね……。",
            "voice": "audition/story_01/heroine_06/HM_Sub_01_06_09/hm_sub_01_06_09_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "値段もそこそこするし、\n今月余裕あるわけでもないし……。",
            "voice": "audition/story_01/heroine_06/HM_Sub_01_06_09/hm_sub_01_06_09_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "でも、買わなかったら、しばらく引きずっちゃうかも。",
            "voice": "audition/story_01/heroine_06/HM_Sub_01_06_09/hm_sub_01_06_09_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "うーん……よし、買っちゃおう。\nでも、今月は出費を切り詰めないと……。",
            "voice": "audition/story_01/heroine_06/HM_Sub_01_06_09/hm_sub_01_06_09_005.mp3"
        }
    ]
}