{
    "Txt_Name": "HM_Cast_01_06_07_04",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "お疲れ様です、マネージャーさん。\n……ふわぁ。",
            "voice": "audition/story_01/heroine_06/HM_Cast_01_06_07_04/hm_cast_01_06_07_04_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "お疲れ様、あいり。\n眠そうだけど、大丈夫？",
            "voice": ""
        },
        {
            "index": 3,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "あ、すみません！\nだ、大丈夫です！",
            "voice": "audition/story_01/heroine_06/HM_Cast_01_06_07_04/hm_cast_01_06_07_04_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "どうしたの、寝不足？",
            "voice": ""
        },
        {
            "index": 5,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "あ、いえ、そういうわけじゃないんです。\n今日はその、4時に起きちゃって……。",
            "voice": "audition/story_01/heroine_06/HM_Cast_01_06_07_04/hm_cast_01_06_07_04_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "4時！？\nそれは早いね……。",
            "voice": ""
        },
        {
            "index": 7,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "もう1度寝ようとしたんですけど、全然ダメで……。",
            "voice": "audition/story_01/heroine_06/HM_Cast_01_06_07_04/hm_cast_01_06_07_04_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "だから、その分、今日はちょっと凝ったお弁当を作りました！！",
            "voice": "audition/story_01/heroine_06/HM_Cast_01_06_07_04/hm_cast_01_06_07_04_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "ウィンナーをタコさんの形にしたんです。\n黒ゴマで、ちゃんと目も付けたんですよ。",
            "voice": "audition/story_01/heroine_06/HM_Cast_01_06_07_04/hm_cast_01_06_07_04_009.mp3"
        }
    ]
}