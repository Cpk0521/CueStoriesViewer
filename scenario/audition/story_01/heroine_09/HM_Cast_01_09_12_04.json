{
    "Txt_Name": "HM_Cast_01_09_12_04",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "お疲れ様ー！",
            "voice": "audition/story_01/heroine_09/HM_Cast_01_09_12_04/hm_cast_01_09_12_04_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "お疲れ様、莉子。",
            "voice": ""
        },
        {
            "index": 3,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "ね、マネージャー。\nここで美晴、待たせてもらっていいかな？",
            "voice": "audition/story_01/heroine_09/HM_Cast_01_09_12_04/hm_cast_01_09_12_04_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "うん、いいよ。2人で一緒に帰るの？",
            "voice": ""
        },
        {
            "index": 5,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "ま、そんなとこ。\n一緒に帰るっていうか、その途中に軽く飲もうって。",
            "voice": "audition/story_01/heroine_09/HM_Cast_01_09_12_04/hm_cast_01_09_12_04_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "今日って、絢とまほろは仕事でさ。\n帰りに一緒に食べてくるって言うからね。",
            "voice": "audition/story_01/heroine_09/HM_Cast_01_09_12_04/hm_cast_01_09_12_04_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "もう美晴が\n「絢とまほろいいな。わたしたちも一緒に行きましょう」って。",
            "voice": "audition/story_01/heroine_09/HM_Cast_01_09_12_04/hm_cast_01_09_12_04_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "莉子ー、お待たせ。\nマネージャーさん、お疲れ様です。",
            "voice": "audition/story_01/heroine_09/HM_Cast_01_09_12_04/hm_cast_01_09_12_04_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "お疲れ様、美晴。\nそれじゃあ、2人楽しんできてね。",
            "voice": ""
        },
        {
            "index": 10,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "はーい。お先に失礼します、マネージャー。",
            "voice": "audition/story_01/heroine_09/HM_Cast_01_09_12_04/hm_cast_01_09_12_04_010.mp3"
        }
    ]
}