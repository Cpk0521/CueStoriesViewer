{
    "Txt_Name": "HM_Sub_01_09_22",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "オーディションのお話をいただいてから、\nもう随分と経ちましたね。",
            "voice": "audition/story_01/heroine_09/HM_Sub_01_09_22/hm_sub_01_09_22_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "毎日、日記をつけているから、\n１日、１日過ぎていくことは実感していたつもりですけど、",
            "voice": "audition/story_01/heroine_09/HM_Sub_01_09_22/hm_sub_01_09_22_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "改めて、今日という現在地を見てしまうと、残り僅かなんだなって……。",
            "voice": "audition/story_01/heroine_09/HM_Sub_01_09_22/hm_sub_01_09_22_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "なーんて、うふふ。\nちょっと浸りすぎかしら。",
            "voice": "audition/story_01/heroine_09/HM_Sub_01_09_22/hm_sub_01_09_22_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "でも、その残り僅かも頑張りますね、マネージャーさん。",
            "voice": "audition/story_01/heroine_09/HM_Sub_01_09_22/hm_sub_01_09_22_005.mp3"
        }
    ]
}