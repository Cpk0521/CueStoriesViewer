{
    "Txt_Name": "HM_Cast_01_12_09_04",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "お疲れ様です。",
            "voice": "audition/story_01/heroine_12/HM_Cast_01_12_09_04/hm_cast_01_12_09_04_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "お疲れ様、マネージャー。",
            "voice": "audition/story_01/heroine_12/HM_Cast_01_12_09_04/hm_cast_01_12_09_04_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "あ、お疲れ様。",
            "voice": ""
        },
        {
            "index": 4,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "んーっ、今日も頑張ったー。",
            "voice": "audition/story_01/heroine_12/HM_Cast_01_12_09_04/hm_cast_01_12_09_04_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "ねぇ、莉子。\n今日は早く帰らないとって、さっき言ってなかった？",
            "voice": "audition/story_01/heroine_12/HM_Cast_01_12_09_04/hm_cast_01_12_09_04_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "え？\n……あ、そうだった！！",
            "voice": "audition/story_01/heroine_12/HM_Cast_01_12_09_04/hm_cast_01_12_09_04_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "早く帰らないと、宅配の人来ちゃう！",
            "voice": "audition/story_01/heroine_12/HM_Cast_01_12_09_04/hm_cast_01_12_09_04_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "あら。\n今からでも、絢かまほろに頼んでみれば？",
            "voice": "audition/story_01/heroine_12/HM_Cast_01_12_09_04/hm_cast_01_12_09_04_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "それでもいいんだけど……\n今日届くの、予約待ち3カ月の超おいしいやつなんだ！",
            "voice": "audition/story_01/heroine_12/HM_Cast_01_12_09_04/hm_cast_01_12_09_04_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "美晴も早く食べたいでしょ？\nほら、一緒に帰ろ！",
            "voice": "audition/story_01/heroine_12/HM_Cast_01_12_09_04/hm_cast_01_12_09_04_010.mp3"
        }
    ]
}