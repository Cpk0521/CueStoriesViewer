        {
            "index": 3,
            "heroineId": [
                10
            ],
            "name": {
                "default": "神室絢"
            },
            "message": {
                "default": "あ、これ知ってる。美晴が好きそうだなって思ってたんだ。"
            },
            "voice": "audition/story_01/heroine_10/HM_Cast_01_10_09_03/hm_cast_01_10_09_03_003.mp3"
