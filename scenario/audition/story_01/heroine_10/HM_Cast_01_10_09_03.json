            "voice": "audition/story_01/heroine_10/HM_Cast_01_10_09_03/hm_cast_01_10_09_03_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                10
            ],
            "name": {
                "default": "神室絢"
            },
            "message": {
