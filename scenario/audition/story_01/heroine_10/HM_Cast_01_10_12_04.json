{
    "Txt_Name": "HM_Cast_01_10_12_04",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "お疲れ様です。\nあ、莉子。",
            "voice": "audition/story_01/heroine_10/HM_Cast_01_10_12_04/hm_cast_01_10_12_04_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "お、絢じゃん。\nお疲れ様ー、はいこれチョコ。",
            "voice": "audition/story_01/heroine_10/HM_Cast_01_10_12_04/hm_cast_01_10_12_04_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "あ、ありがとう。\n……ねぇ莉子、なんで最近よくお菓子くれるの？",
            "voice": "audition/story_01/heroine_10/HM_Cast_01_10_12_04/hm_cast_01_10_12_04_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "やっぱ気付いてた？\n実は最近、バイト先の人からやたらお菓子貰っててさ。",
            "voice": "audition/story_01/heroine_10/HM_Cast_01_10_12_04/hm_cast_01_10_12_04_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あたし1人だと食べきれないし、\nおすそ分けしようと思って。",
            "voice": "audition/story_01/heroine_10/HM_Cast_01_10_12_04/hm_cast_01_10_12_04_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "そうだったんだ。\n言ってくれればよかったのに。",
            "voice": "audition/story_01/heroine_10/HM_Cast_01_10_12_04/hm_cast_01_10_12_04_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あはは、ごめんごめん。\nなんか絢ってさ、物あげたくなっちゃうんだよねー。",
            "voice": "audition/story_01/heroine_10/HM_Cast_01_10_12_04/hm_cast_01_10_12_04_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "飴玉を持ち歩くおばちゃんの気持ちがわかったよ、うん。",
            "voice": "audition/story_01/heroine_10/HM_Cast_01_10_12_04/hm_cast_01_10_12_04_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "……？\nよくわからないけど、ありがと。",
            "voice": "audition/story_01/heroine_10/HM_Cast_01_10_12_04/hm_cast_01_10_12_04_009.mp3"
        }
    ]
}