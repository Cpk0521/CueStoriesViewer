{
    "Txt_Name": "HM_Main_01_07_01",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                101
            ],
            "name": "鳳真咲",
            "message": "おはよう、マネージャー。\nちょうど良かったわ。",
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_01/hm_main_01_07_01_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                101
            ],
            "name": "鳳真咲",
            "message": "今度、あいりをこのオーディションに出してみようと思うの。",
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_01/hm_main_01_07_01_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "「空繰戯曲グレイゼファー」……、ですか。",
            "voice": ""
        },
        {
            "index": 4,
            "heroineId": [
                101
            ],
            "name": "鳳真咲",
            "message": "そう、あの子にちょうど合うと思って。",
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_01/hm_main_01_07_01_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                101
            ],
            "name": "鳳真咲",
            "message": "りおにもサポートするように言ってあるから、よろしくね。",
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_01/hm_main_01_07_01_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                102
            ],
            "name": "五十鈴りお",
            "message": "そういうこと！\n頑張ってね！",
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_01/hm_main_01_07_01_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "え？　オーディションですか？",
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_01/hm_main_01_07_01_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "そう。社長の推薦だよ。",
            "voice": ""
        },
        {
            "index": 9,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "す、推薦……！",
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_01/hm_main_01_07_01_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "ん？　どうしたの？",
            "voice": ""
        },
        {
            "index": 11,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "あ、ごめんなさい。なんでもないんです。\nただ……。",
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_01/hm_main_01_07_01_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "あいり、ちゃんとできるかなって……。",
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_01/hm_main_01_07_01_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "大丈夫だよ。\n自分もサポートするから、一緒に頑張ろうね。",
            "voice": ""
        },
        {
            "index": 14,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "は、はい……！　あいり、頑張ります。\nよろしくお願いします、マネージャーさん。",
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_01/hm_main_01_07_01_014.mp3"
        }
    ]
}