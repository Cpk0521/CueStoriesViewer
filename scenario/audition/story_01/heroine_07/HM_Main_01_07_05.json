        },
        {
            "index": 8,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "どんなに怖くても、1歩を踏み出すと思います。",
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_05/hm_main_01_07_05_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "あいりも、リボン・リーみたいに……。",
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_05/hm_main_01_07_05_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "うん。あいりならできるよ。",
            "voice": ""
        },
        {
            "index": 11,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "……はい！\nありがとうございます、マネージャーさん！",
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_05/hm_main_01_07_05_011.mp3"
        }
