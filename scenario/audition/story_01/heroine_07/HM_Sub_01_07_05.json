{
    "Txt_Name": "HM_Sub_01_07_05",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "マネージャーさんは、\nプチキュアって観たことありますか？",
            "voice": "audition/story_01/heroine_07/HM_Sub_01_07_05/hm_sub_01_07_05_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "子ども向けのアニメですけど、\nお話はとってもとっても深いんですよ。",
            "voice": "audition/story_01/heroine_07/HM_Sub_01_07_05/hm_sub_01_07_05_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "それに、ドキドキの展開の中にも、ちゃんと夢があるんです。",
            "voice": "audition/story_01/heroine_07/HM_Sub_01_07_05/hm_sub_01_07_05_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "プチキュアって、本当にすごいんです。\nだから、マネージャーさん……。",
            "voice": "audition/story_01/heroine_07/HM_Sub_01_07_05/hm_sub_01_07_05_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "今度、観てみてください！\nそれで、あいりと一緒にお話ししましょう！",
            "voice": "audition/story_01/heroine_07/HM_Sub_01_07_05/hm_sub_01_07_05_005.mp3"
        }
    ]
}