{
    "Txt_Name": "HM_Cast_01_07_06_01",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "お疲れ様です、マネージャー。\nちょっとここで時間つぶしていってもいいですか？",
            "voice": "audition/story_01/heroine_07/HM_Cast_01_07_06_01/hm_cast_01_07_06_01_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "うん、大丈夫だよ。\nこのあと、なにかあるの？",
            "voice": ""
        },
        {
            "index": 3,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "スーパーのタイムセールです。",
            "voice": "audition/story_01/heroine_07/HM_Cast_01_07_06_01/hm_cast_01_07_06_01_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "ああ、なるほど。\n頑張ってね。",
            "voice": ""
        },
        {
            "index": 5,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "はい！\nっと、今日の目玉はなんだっけ……。",
            "voice": "audition/story_01/heroine_07/HM_Cast_01_07_06_01/hm_cast_01_07_06_01_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "白菜と、卵と、ソーセージ……。うーん、\nソーセージは2袋くらい買っておいたほうがいいかしら……。",
            "voice": "audition/story_01/heroine_07/HM_Cast_01_07_06_01/hm_cast_01_07_06_01_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "あら、プチキュアカレー、安いじゃない。",
            "voice": "audition/story_01/heroine_07/HM_Cast_01_07_06_01/hm_cast_01_07_06_01_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "今月はちょっと余裕あるし……。\nあいり、これ買ったら喜ぶかな。",
            "voice": "audition/story_01/heroine_07/HM_Cast_01_07_06_01/hm_cast_01_07_06_01_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "うーん……悩ましいわね。",
            "voice": "audition/story_01/heroine_07/HM_Cast_01_07_06_01/hm_cast_01_07_06_01_009.mp3"
        }
    ]
}