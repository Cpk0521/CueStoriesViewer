{
    "Txt_Name": "HM_Main_01_07_03",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "あいり！\n1次審査、合格おめでとう！",
            "voice": ""
        },
        {
            "index": 2,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "はい！\nありがとうございます、マネージャーさん。",
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_03/hm_main_01_07_03_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "合格できて、本当に良かったです……。\nはぁ……。",
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_03/hm_main_01_07_03_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "ちょっとホッとした？",
            "voice": ""
        },
        {
            "index": 5,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "えへへ……はい。\nまだ1次なのはわかってるんですけど、嬉しくて。",
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_03/hm_main_01_07_03_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "うんうん。この調子で、次の審査も頑張ろうね。\nあいりなら大丈夫だよ。",
            "voice": ""
        },
        {
            "index": 7,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "は、はい……。\nマネージャーさんがそう言ってくれると、ちょっと安心します……。",
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_03/hm_main_01_07_03_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "ううっ、でも……。",
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_03/hm_main_01_07_03_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "どうしたの？",
            "voice": ""
        },
        {
            "index": 10,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "ごめんなさい……これは、あいりの悪い癖なんです。",
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_03/hm_main_01_07_03_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "いいことがあると、今度は逆に、アンラッキーなことが\n起きるんじゃないかって……つい考えちゃって。",
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_03/hm_main_01_07_03_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "あいり、次もちゃんとできるのかな……。",
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_03/hm_main_01_07_03_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "うう……マネージャーさん。\nどんどん不安になってきました。",
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_03/hm_main_01_07_03_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "マネージャーさん……！",
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_03/hm_main_01_07_03_015.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "ありがとうございます。\nとっても心強いです！",
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_03/hm_main_01_07_03_016.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "ううっ……。\nやっぱり、本番でアンラッキーなことが……。",
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_03/hm_main_01_07_03_018.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "あいり、明日神社にお参りに行ってきます……！",
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_03/hm_main_01_07_03_019.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "ええっ！　それはダメです！",
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_03/hm_main_01_07_03_021.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "マネージャーさんが不幸になるほうが、あいりは嫌です。",
            "voice": "audition/story_01/heroine_07/HM_Main_01_07_03/hm_main_01_07_03_022.mp3"
        }
    ]
}