    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "お疲れ様です。",
            "voice": "audition/story_01/heroine_02/HM_Cast_01_02_01_01/hm_cast_01_02_01_01_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "お疲れ様でーす。\nじゃ、行こっか、陽菜。",
            "voice": "audition/story_01/heroine_02/HM_Cast_01_02_01_01/hm_cast_01_02_01_01_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "あ、ちょっと待ってて、舞花ちゃん。\nマネージャーさん、ちょっといいですか？",
            "voice": "audition/story_01/heroine_02/HM_Cast_01_02_01_01/hm_cast_01_02_01_01_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "学校に提出する書類に、事務所の印が必要で……。\n結構あるんですけど、押印お願いできますか？",
            "voice": "audition/story_01/heroine_02/HM_Cast_01_02_01_01/hm_cast_01_02_01_01_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "ん、わかった。確認して押印しとくね。",
            "voice": ""
        },
        {
            "index": 6,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "へぇ、さすが私立は違うなー。\n自分の学校、あんまこういうのなかったし。",
            "voice": "audition/story_01/heroine_02/HM_Cast_01_02_01_01/hm_cast_01_02_01_01_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "え？　舞花たちの学校もあったよ。",
            "voice": ""
        },
        {
            "index": 8,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "え？\nあはは、そうでしたっけ？",
            "voice": "audition/story_01/heroine_02/HM_Cast_01_02_01_01/hm_cast_01_02_01_01_008.mp3"
        }
    ]
}