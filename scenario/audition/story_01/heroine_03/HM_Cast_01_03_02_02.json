{
    "Txt_Name": "HM_Cast_01_03_02_02",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "お疲れ様でーす。",
            "voice": "audition/story_01/heroine_03/HM_Cast_01_03_02_02/hm_cast_01_03_02_02_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "お疲れお疲れ。",
            "voice": "audition/story_01/heroine_03/HM_Cast_01_03_02_02/hm_cast_01_03_02_02_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "あ、そうだ、志穂。\n今日、何食べたい？",
            "voice": "audition/story_01/heroine_03/HM_Cast_01_03_02_02/hm_cast_01_03_02_02_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "何にしようか決めてないんだよね。",
            "voice": "audition/story_01/heroine_03/HM_Cast_01_03_02_02/hm_cast_01_03_02_02_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "本当か。じゃあ、オムライス。",
            "voice": "audition/story_01/heroine_03/HM_Cast_01_03_02_02/hm_cast_01_03_02_02_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "オムライスかー……今、卵が少ないから他のがいいなぁ。",
            "voice": "audition/story_01/heroine_03/HM_Cast_01_03_02_02/hm_cast_01_03_02_02_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "なら、唐揚げ。",
            "voice": "audition/story_01/heroine_03/HM_Cast_01_03_02_02/hm_cast_01_03_02_02_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "んー、今日、鳥より豚のほうが安いんだよなー。",
            "voice": "audition/story_01/heroine_03/HM_Cast_01_03_02_02/hm_cast_01_03_02_02_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "じゃあ、もうなんでもいい。",
            "voice": "audition/story_01/heroine_03/HM_Cast_01_03_02_02/hm_cast_01_03_02_02_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "えー、なんでもいいだと困るんだよなー。\nほら、なんかない？",
            "voice": "audition/story_01/heroine_03/HM_Cast_01_03_02_02/hm_cast_01_03_02_02_010.mp3"
        }
    ]
}