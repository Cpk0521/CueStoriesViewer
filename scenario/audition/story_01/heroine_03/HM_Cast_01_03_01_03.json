{
    "Txt_Name": "HM_Cast_01_03_01_03",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "お疲れちゃーん。",
            "voice": "audition/story_01/heroine_03/HM_Cast_01_03_01_03/hm_cast_01_03_01_03_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "お疲れ様。\nそうだ、志穂。今ちょっといい？",
            "voice": ""
        },
        {
            "index": 3,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "む。\n本当にちょっとなら平気。",
            "voice": "audition/story_01/heroine_03/HM_Cast_01_03_01_03/hm_cast_01_03_01_03_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "え？　なにか用事でも？",
            "voice": ""
        },
        {
            "index": 5,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "亀井さんの餌やりを頼まれたんだ。\n今日の陽菜、遅いからな。",
            "voice": "audition/story_01/heroine_03/HM_Cast_01_03_01_03/hm_cast_01_03_01_03_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "なるほどね。\n手伝いがあるってことか。",
            "voice": ""
        },
        {
            "index": 7,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "いや、これはギブアンドテイク。",
            "voice": "audition/story_01/heroine_03/HM_Cast_01_03_01_03/hm_cast_01_03_01_03_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "代わりに、今度作品のモデルになってもらう予定。",
            "voice": "audition/story_01/heroine_03/HM_Cast_01_03_01_03/hm_cast_01_03_01_03_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "なので、ジャーマネは手短に話してほしい。",
            "voice": "audition/story_01/heroine_03/HM_Cast_01_03_01_03/hm_cast_01_03_01_03_009.mp3"
        }
    ]
}