{
    "Txt_Name": "Card_4150011_2",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "みんな、おはよー！",
            "voice": "card/Card_15/Card_4150011_2/card_4150011_2_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "いい朝だねー！",
            "voice": "card/Card_15/Card_4150011_2/card_4150011_2_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "あれ？　リエンヌ？　どうしたの？",
            "voice": "card/Card_15/Card_4150011_2/card_4150011_2_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "行っちゃった……。",
            "voice": "card/Card_15/Card_4150011_2/card_4150011_2_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "あ、さとちん！　リエンヌ、なんか変だったねー。",
            "voice": "card/Card_15/Card_4150011_2/card_4150011_2_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "あれ？　さとちんも、なんか変だねー？",
            "voice": "card/Card_15/Card_4150011_2/card_4150011_2_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "さとちんも行っちゃった……。",
            "voice": "card/Card_15/Card_4150011_2/card_4150011_2_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "あ、なるちゃん、おはよー！\nあのね、リエンヌとさとちんがね……。",
            "voice": "card/Card_15/Card_4150011_2/card_4150011_2_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "なるちゃんも行っちゃった……。",
            "voice": "card/Card_15/Card_4150011_2/card_4150011_2_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "……。",
            "voice": "card/Card_15/Card_4150011_2/card_4150011_2_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "なんか、みんな、おかしい！",
            "voice": "card/Card_15/Card_4150011_2/card_4150011_2_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "利恵。",
            "voice": "card/Card_15/Card_4150011_2/card_4150011_2_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "わかってる……。我輩も、これ以上は……。",
            "voice": "card/Card_15/Card_4150011_2/card_4150011_2_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "なかなか来るものがあるわね……。",
            "voice": "card/Card_15/Card_4150011_2/card_4150011_2_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "でも、19時まで！　\n19時まではグッと我慢だから！",
            "voice": "card/Card_15/Card_4150011_2/card_4150011_2_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "……。",
            "voice": "card/Card_15/Card_4150011_2/card_4150011_2_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "なんだかみんなおかしいよ……。\nどうしたんだろう。",
            "voice": "card/Card_15/Card_4150011_2/card_4150011_2_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "あんな風に何も答えてくれないってこと、\n今まで一回もなかったし……。",
            "voice": "card/Card_15/Card_4150011_2/card_4150011_2_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "いつもみんな忙しそうだし疲れてるのかな……。",
            "voice": "card/Card_15/Card_4150011_2/card_4150011_2_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "でも、今日のみんな、疲れてるって言うよりもまるで、\nなんだかわたしの声が聞こえてないみたいな……。",
            "voice": "card/Card_15/Card_4150011_2/card_4150011_2_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "今までのみんなじゃないみたいな……。",
            "voice": "card/Card_15/Card_4150011_2/card_4150011_2_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "え？　もしかしてみんな……。",
            "voice": "card/Card_15/Card_4150011_2/card_4150011_2_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "もしかして、もしかして！",
            "voice": "card/Card_15/Card_4150011_2/card_4150011_2_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "この前さとちんと見た映画みたいに、\n悪霊に取り憑かれてるのかも……！",
            "voice": "card/Card_15/Card_4150011_2/card_4150011_2_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "そんなー！　どうしよー！",
            "voice": "card/Card_15/Card_4150011_2/card_4150011_2_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "あ、でも、リエンヌは悪魔なのに、\n悪霊に取り憑かれたりするのかな？",
            "voice": "card/Card_15/Card_4150011_2/card_4150011_2_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "だけど、リエンヌに悪霊ってかなりすごいんじゃ！",
            "voice": "card/Card_15/Card_4150011_2/card_4150011_2_027.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "そんなすごい悪霊に取り憑かれてるんだとしたら、\nわたしがなんとかしないと！",
            "voice": "card/Card_15/Card_4150011_2/card_4150011_2_028.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "いつもみんなには、助けてもらってばかりなんだから！",
            "voice": "card/Card_15/Card_4150011_2/card_4150011_2_029.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "うーん。\nでも一体どうやって？",
            "voice": "card/Card_15/Card_4150011_2/card_4150011_2_030.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "あの映画だとお祈りみたいなことしてたけど……。",
            "voice": "card/Card_15/Card_4150011_2/card_4150011_2_031.mp3"
        },
        {
            "index": 32,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "あ、そういえば！",
            "voice": "card/Card_15/Card_4150011_2/card_4150011_2_032.mp3"
        },
        {
            "index": 33,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "お寺！",
            "voice": "card/Card_15/Card_4150011_2/card_4150011_2_033.mp3"
        },
        {
            "index": 34,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "鳴。凛音は今どこ？　知ってる？",
            "voice": "card/Card_15/Card_4150011_2/card_4150011_2_034.mp3"
        },
        {
            "index": 35,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "志穂さんの部屋。",
            "voice": "card/Card_15/Card_4150011_2/card_4150011_2_035.mp3"
        },
        {
            "index": 36,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "え？　志穂の部屋？　どうしたんだろう。\nもうすぐ19時、そろそろリビングに来てくれないと計画が……。",
            "voice": "card/Card_15/Card_4150011_2/card_4150011_2_036.mp3"
        },
        {
            "index": 37,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "私、呼びに行こうか？",
            "voice": "card/Card_15/Card_4150011_2/card_4150011_2_037.mp3"
        },
        {
            "index": 38,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "あ、凛音……。",
            "voice": "card/Card_15/Card_4150011_2/card_4150011_2_038.mp3"
        },
        {
            "index": 39,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "みんな、ごめんね！",
            "voice": "card/Card_15/Card_4150011_2/card_4150011_2_039.mp3"
        },
        {
            "index": 40,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "え？",
            "voice": "card/Card_15/Card_4150011_2/card_4150011_2_040.mp3"
        },
        {
            "index": 41,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "でも、もう大丈夫！",
            "voice": "card/Card_15/Card_4150011_2/card_4150011_2_041.mp3"
        },
        {
            "index": 42,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "な、何？　どうしたの、凛音。",
            "voice": "card/Card_15/Card_4150011_2/card_4150011_2_042.mp3"
        },
        {
            "index": 43,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "……？",
            "voice": "card/Card_15/Card_4150011_2/card_4150011_2_043.mp3"
        },
        {
            "index": 44,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "ううん。大丈夫。わたし、わかってるから。",
            "voice": "card/Card_15/Card_4150011_2/card_4150011_2_044.mp3"
        },
        {
            "index": 45,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "え？　わかってるの？",
            "voice": "card/Card_15/Card_4150011_2/card_4150011_2_045.mp3"
        },
        {
            "index": 46,
            "heroineId": [
                15
            ],
            "name": "明神凛音",
            "message": "うん。みんな、もう大丈夫だよ！\nすぐ元どおりにしてあげる！",
            "voice": "card/Card_15/Card_4150011_2/card_4150011_2_046.mp3"
        },
        {
            "index": 47,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "え？　元どおりに……、って、どういうこと？",
            "voice": "card/Card_15/Card_4150011_2/card_4150011_2_047.mp3"
        }
    ]
}