            "voice": ""
        },
        {
            "index": 45,
            "heroineId": [
                12
            ],
            "name": {
                "default": "日名倉莉子"
            },
            "message": {
                "default": "すみません、失礼しますっ！"
            },
            "voice": "card/Card_12/Card_3120004_1/card_3120004_1_045.mp3"
        },
        {
            "index": 46,
            "heroineId": [
                0
            ],
            "name": {
                "default": "黒山監督"
            },
            "message": {
                "default": "合格よ。"
            },
            "voice": ""
        },
        {
            "index": 47,
            "heroineId": [
                12
            ],
            "name": {
                "default": "日名倉莉子"
            },
            "message": {
                "default": "へっ！？"
