            "voice": ""
        },
        {
            "index": 50,
            "heroineId": [
                12
            ],
            "name": {
                "default": "日名倉莉子"
            },
            "message": {
                "default": "……という事の次第でして……。"
            },
            "voice": "card/Card_12/Card_3120004_1/card_3120004_1_050.mp3"
