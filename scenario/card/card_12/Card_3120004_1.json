            "voice": "card/Card_12/Card_3120004_1/card_3120004_1_047.mp3"
        },
        {
            "index": 48,
            "heroineId": [
                0
            ],
            "name": {
                "default": "スーツの女性"
            },
            "message": {
                "default": "おめでとうございます。\n最終オーディションに進めましたよ。"
