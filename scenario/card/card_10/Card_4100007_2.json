        },
        {
            "index": 8,
            "heroineId": [
                11
            ],
            "name": {
                "default": "宮路まほろ"
            },
            "message": {
                "default": "……なに？　そんなにショックなわけ？"
            },
            "voice": "card/Card_10/Card_4100007_2/card_4100007_2_008.mp3"
        },
        {
            "index": 9,
