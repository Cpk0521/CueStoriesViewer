{
    "Txt_Name": "Lesson_30407",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                0
            ],
            "name": "ディレクター",
            "message": "はい……、仕方ないでしょう……。\nとりあえず、いただきました。",
            "voice": ""
        },
        {
            "index": 2,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "あ、あの、すみません。\n申し訳ありませんでした。",
            "voice": "lesson/Lesson_30407/lesson_30407_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                0
            ],
            "name": "ディレクター",
            "message": "謝られてもねぇ。ナレーション苦手なの？\n基礎からやりなおしてね。",
            "voice": ""
        },
        {
            "index": 4,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "ちょっとあいり……、大丈夫？",
            "voice": ""
        },
        {
            "index": 5,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "すみません……。",
            "voice": "lesson/Lesson_30407/lesson_30407_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "落ち込まなくていいよ。\nあーいうディレクターもいるからさ。",
            "voice": ""
        },
        {
            "index": 7,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "すみません。",
            "voice": "lesson/Lesson_30407/lesson_30407_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "今日のナレーションの原稿は、あいり向きじゃなかったかも。\nごめんね。",
            "voice": ""
        },
        {
            "index": 9,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "すみません……。",
            "voice": "lesson/Lesson_30407/lesson_30407_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "たまたま合わなかっただけだよ。言われたことは参考にしてさ。\nこれからまた頑張ればいいじゃない。",
            "voice": ""
        },
        {
            "index": 11,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "はい……。すみません。",
            "voice": "lesson/Lesson_30407/lesson_30407_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "（まいったなぁ。今日は何を言ってもダメか。）",
            "voice": ""
        },
        {
            "index": 13,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "また機会はあるから。落ち込み過ぎないでね。",
            "voice": ""
        },
        {
            "index": 14,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "すみません。",
            "voice": "lesson/Lesson_30407/lesson_30407_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "やっぱり……、向いてないのかなぁ。",
            "voice": "lesson/Lesson_30407/lesson_30407_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "アイリいる？　",
            "voice": "lesson/Lesson_30407/lesson_30407_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "あ、柚葉ちゃん。どうぞ。",
            "voice": "lesson/Lesson_30407/lesson_30407_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "私のお店で、明日、バーゲンやるの。クリスマス前の大放出！\nよかったら来ない？",
            "voice": "lesson/Lesson_30407/lesson_30407_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "あ……、明日はちょっと。",
            "voice": "lesson/Lesson_30407/lesson_30407_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "アイリに似合いそうなアイテム、沢山あるんだけどなぁ。",
            "voice": "lesson/Lesson_30407/lesson_30407_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "ごめんね……。",
            "voice": "lesson/Lesson_30407/lesson_30407_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "オーケー！　悠希と千紗も来るからさ。もし予定変わったら来てね！",
            "voice": "lesson/Lesson_30407/lesson_30407_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "はぁ……、みんなで休日かぁ……。",
            "voice": "lesson/Lesson_30407/lesson_30407_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "なんであいりはダメなんだろう……、ますます落ち込むよぉ……。",
            "voice": "lesson/Lesson_30407/lesson_30407_024.mp3"
        }
    ]
}