{
    "Txt_Name": "Lesson_31503",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "ねえ美晴、あの歌……。",
            "voice": ""
        },
        {
            "index": 2,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "はい。",
            "voice": "lesson/Lesson_31503/lesson_31503_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "自分がちゃんと覚えるまで何度でも歌うって、\n美晴は言ってくれたけど……。",
            "voice": ""
        },
        {
            "index": 4,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "自分はやっぱり、歌はあまり上手くないから。\n録音して正解だったかも。",
            "voice": ""
        },
        {
            "index": 5,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "せっかくだからマネージャーさんも覚えてくれたらいいのに。",
            "voice": "lesson/Lesson_31503/lesson_31503_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "自分の声より美晴のほうがいい声だしね。",
            "voice": ""
        },
        {
            "index": 7,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "そうですか？\nマネージャーさんもいい声だと思いますよ？",
            "voice": "lesson/Lesson_31503/lesson_31503_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "あはは。ありがとう。\nでもあの歌、とてもいい歌だね。",
            "voice": ""
        },
        {
            "index": 9,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "昔、教えてもらったって言ってたけど、\nその人のオリジナルなの？",
            "voice": ""
        },
        {
            "index": 10,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "さあ、どうでしょう。\n詳しくはわたしも知らないんです。うふふ。",
            "voice": "lesson/Lesson_31503/lesson_31503_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "いつか会う時があったら、お礼を伝えてほしいな。\nとても素敵な曲をありがとうございますって。",
            "voice": ""
        },
        {
            "index": 12,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "それ以上の言葉はありませんね。\nきっと……、喜ぶと思います。",
            "voice": "lesson/Lesson_31503/lesson_31503_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "そう。それなら良かった。",
            "voice": ""
        },
        {
            "index": 14,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "はい。\nきっと……。",
            "voice": "lesson/Lesson_31503/lesson_31503_014.mp3"
        }
    ]
}