{
    "Txt_Name": "Lesson_31108",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "はあ、はあ。\n足が……、もう、棒みたい。",
            "voice": "lesson/Lesson_31108/lesson_31108_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "（もう、ダメかも。）",
            "voice": "lesson/Lesson_31108/lesson_31108_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "（でも、あいり、こんなに走れた。）",
            "voice": "lesson/Lesson_31108/lesson_31108_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "（えへへ。\nちょっと前だったら、こんなには走れなかったよ。）",
            "voice": "lesson/Lesson_31108/lesson_31108_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "（その前に、40階以上も階段上るなんて出来なかったな。\n……ずっとみんなとトレーニングしてきたからだよね。）",
            "voice": "lesson/Lesson_31108/lesson_31108_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "（それに、怪我もしてないよ……。）",
            "voice": "lesson/Lesson_31108/lesson_31108_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "（怪我なんかしたら、絶対、2人とも心配するもん。）",
            "voice": "lesson/Lesson_31108/lesson_31108_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "（千紗ちゃんは、思いっきり、わかりやすく。\n悠希ちゃんは、きっと……。）",
            "voice": "lesson/Lesson_31108/lesson_31108_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "はあ、はあ……。　",
            "voice": "lesson/Lesson_31108/lesson_31108_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "（どのくらい下りられたかな……。）",
            "voice": "lesson/Lesson_31108/lesson_31108_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "（もう、大丈夫かな……。\nあいり、そろそろ限界……。）",
            "voice": "lesson/Lesson_31108/lesson_31108_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "（悠希ちゃん……、千紗ちゃん……。\nちゃんと王子の部屋に入れたかな？）",
            "voice": "lesson/Lesson_31108/lesson_31108_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "（ごめんね。\nあいり、もう、このへんで、ゴール、かも。）",
            "voice": "lesson/Lesson_31108/lesson_31108_013.mp3"
        }
    ]
}