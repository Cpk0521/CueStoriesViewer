{
    "Txt_Name": "Lesson_30411",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "さーて！　今日はパッと終わらせて、パーティーに行くわよ！",
            "voice": "lesson/Lesson_30411/lesson_30411_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "一応言っておくけど……。\nパーティーに行きたいからって、仕事の手は抜かないでよ？",
            "voice": "lesson/Lesson_30411/lesson_30411_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "もちろんよ！　リーディングライブも大成功で、\nパーティーも大盛りあがりで、サイッコウの1日にするわ！",
            "voice": "lesson/Lesson_30411/lesson_30411_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "柚葉ちゃん、お願いね。",
            "voice": "lesson/Lesson_30411/lesson_30411_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "大丈夫大丈夫！　私に任せといて！\n完璧なリーディングライブにしてみせるわ！",
            "voice": "lesson/Lesson_30411/lesson_30411_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "その自信が逆に不安だわ……。",
            "voice": "lesson/Lesson_30411/lesson_30411_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "（うーん、予定よりもだいぶ時間押してるし、\nライブもしんみりした感じだわ。）",
            "voice": "lesson/Lesson_30411/lesson_30411_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "（子どもたちも楽しくなさそうだし、\nこれ、やる意味あるのかしら？）",
            "voice": "lesson/Lesson_30411/lesson_30411_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "（あっ！　ユーキがアドリブを入れて面白くしてくれた！\nこれに乗っかって……！）",
            "voice": "lesson/Lesson_30411/lesson_30411_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "わかんないけど、面白そう！　イッツ、ショータイム！",
            "voice": "lesson/Lesson_30411/lesson_30411_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "（やっぱり、難しく考えずに、楽しむのが1番よね！）",
            "voice": "lesson/Lesson_30411/lesson_30411_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "（このままみんなで踊って、それで終わりでいいじゃない！）",
            "voice": "lesson/Lesson_30411/lesson_30411_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "柚葉ちゃん、お話終わっちゃう！？",
            "voice": "lesson/Lesson_30411/lesson_30411_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "いいじゃん、だってみんなノリノリだよ？\nイェーイ！",
            "voice": "lesson/Lesson_30411/lesson_30411_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "（さっきより、全然みんな楽しそうだし、\n後は思い思いの楽しいことをすればいいのよ！）",
            "voice": "lesson/Lesson_30411/lesson_30411_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "（……いつの間にか、お話に戻っちゃった。\nパーティー、もう始まってるだろうなぁ。）",
            "voice": "lesson/Lesson_30411/lesson_30411_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "（あっ、スマホに着信がある。\nきっとパーティー会場からだわ！）",
            "voice": "lesson/Lesson_30411/lesson_30411_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "sorry 私の電話。",
            "voice": "lesson/Lesson_30411/lesson_30411_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "緊急だと思うの。",
            "voice": "lesson/Lesson_30411/lesson_30411_020.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "じゃあ……、仕方ないかぁ。",
            "voice": "lesson/Lesson_30411/lesson_30411_021.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "仕方ないわけないでしょっ。",
            "voice": "lesson/Lesson_30411/lesson_30411_022.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "（早くしないと、パーティーが終わっちゃう。\nでも、リーディングライブもちゃんと終わらせないと。）",
            "voice": "lesson/Lesson_30411/lesson_30411_023.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "『ここで祈ろう。そして鐘を鳴らしてハッピーエンドだ！』",
            "voice": "lesson/Lesson_30411/lesson_30411_024.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "雑！　終わらせ方雑よ！",
            "voice": "lesson/Lesson_30411/lesson_30411_025.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "（難しいわね……。友達からも電話がいっぱい来てるし、\nどうしたらいいんだろう？）",
            "voice": "lesson/Lesson_30411/lesson_30411_026.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "（しょうがない……、電話は諦めて\nあとでメールしとこ。）",
            "voice": "lesson/Lesson_30411/lesson_30411_027.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "（でも、まだ私は諦めないわ！\n大切な友達が待っていてくれてるんだもの！）",
            "voice": "lesson/Lesson_30411/lesson_30411_028.mp3"
        }
    ]
}