{
    "Txt_Name": "Lesson_31804",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "(なるほど。\n私の役はこういう人物ね。)",
            "voice": "lesson/Lesson_31804/lesson_31804_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "(誰よりも才能がありながら二番弟子の地位に甘んじている。)",
            "voice": "lesson/Lesson_31804/lesson_31804_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "(それでゲームの勝利目標は……。)",
            "voice": "lesson/Lesson_31804/lesson_31804_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "(犯人の推理、後継者を決める何らかの遺言の謎を解く。\n自分が後継者になる……。)",
            "voice": "lesson/Lesson_31804/lesson_31804_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "(これはどういうこと？)",
            "voice": "lesson/Lesson_31804/lesson_31804_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "(後継者を決める何らかの遺言って例えば遺書のような……。)",
            "voice": "lesson/Lesson_31804/lesson_31804_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "(でもそれとは別に自分が後継者になるという目標がある……。)",
            "voice": "lesson/Lesson_31804/lesson_31804_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "(それはきっと、遺言の内容そのままでは、\n自分が後継者じゃないということかしら。)",
            "voice": "lesson/Lesson_31804/lesson_31804_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "(それで何かしらの細工をして自分が後継者になる事もできると。)",
            "voice": "lesson/Lesson_31804/lesson_31804_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "(なるほど。)",
            "voice": "lesson/Lesson_31804/lesson_31804_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "(マネージャーさん、一晩でシナリオ考えてきたと言ってたけど、\n情報の出し方のツメが甘いわね。)",
            "voice": "lesson/Lesson_31804/lesson_31804_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "(それで……。\n事前の情報だと、現状、一番弟子が最も後継者に近いわけね。)",
            "voice": "lesson/Lesson_31804/lesson_31804_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "(ふーん。\nいいわ。面白そうじゃない。)",
            "voice": "lesson/Lesson_31804/lesson_31804_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "(でも、こういうの凛音できるのかしら。\n不安ね……。)",
            "voice": "lesson/Lesson_31804/lesson_31804_014.mp3"
        }
    ]
}