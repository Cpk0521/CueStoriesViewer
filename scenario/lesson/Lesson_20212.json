{
    "Txt_Name": "Lesson_20212",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "楽しみだなぁポンコツラーメン。",
            "voice": "lesson/Lesson_20212/lesson_20212_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "すっごく美味しいよ。",
            "voice": "lesson/Lesson_20212/lesson_20212_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "（2人がとんでもない方向に脱線しちゃってる。\n　これはいよいよ止めないと……。）",
            "voice": "lesson/Lesson_20212/lesson_20212_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "せっかく観光地行くんだし、みんなにお土産買う？",
            "voice": "lesson/Lesson_20212/lesson_20212_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "Good！　買おう。\nそこって、キレイな小物とか売ってるでしょ？",
            "voice": "lesson/Lesson_20212/lesson_20212_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "そういうお店は多いよ。\n和風の小物がたくさん売ってる。",
            "voice": "lesson/Lesson_20212/lesson_20212_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "じゃあ～そこで素敵な文具を買って、\nみんなにプレゼントしちゃおう。喜ぶかなみんな？",
            "voice": "lesson/Lesson_20212/lesson_20212_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "良いと思うよ。",
            "voice": "lesson/Lesson_20212/lesson_20212_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "（とっても嬉しいよ、柚葉ちゃん！）",
            "voice": "lesson/Lesson_20212/lesson_20212_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "よし、そこでオシャレな毛ガニ買っちゃう！",
            "voice": "lesson/Lesson_20212/lesson_20212_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "（毛ガニを！？　人数分買うの！？\n　絶対になにかと間違えてるよ！！）",
            "voice": "lesson/Lesson_20212/lesson_20212_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "もう週末なんて待ってられない、\n今から行っちゃおうか？",
            "voice": "lesson/Lesson_20212/lesson_20212_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "（大変っ、止めてあげなきゃ。）",
            "voice": "lesson/Lesson_20212/lesson_20212_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "ＹＥＳ！！",
            "voice": "lesson/Lesson_20212/lesson_20212_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "浅草寺に行ってとんこつラーメン食べて、\nみんなに手紙買ってあげるツアーへ。出発！",
            "voice": "lesson/Lesson_20212/lesson_20212_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "え……？",
            "voice": "lesson/Lesson_20212/lesson_20212_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "レッツゴー！",
            "voice": "lesson/Lesson_20212/lesson_20212_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "あ……。ちゃんと通じ合ってたんだ。",
            "voice": "lesson/Lesson_20212/lesson_20212_018.mp3"
        }
    ]
}