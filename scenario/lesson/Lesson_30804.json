{
    "Txt_Name": "Lesson_30804",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "あの、すみません。予約してた本を買いに来たんですけど……。",
            "voice": "lesson/Lesson_30804/lesson_30804_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "あ、これ、予約の控えです。",
            "voice": "lesson/Lesson_30804/lesson_30804_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                0
            ],
            "name": "店員",
            "message": "はい。ありがとうございます。こちらですね。\nカバーはおつけいたしますか？",
            "voice": ""
        },
        {
            "index": 4,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "はい。お願いします。",
            "voice": "lesson/Lesson_30804/lesson_30804_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "予約してた新刊、読むの楽しみだなぁ……。\n予約特典も貰っちゃったし。",
            "voice": "lesson/Lesson_30804/lesson_30804_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "さ、帰ってゆっくり読もうっと。",
            "voice": "lesson/Lesson_30804/lesson_30804_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "あれ？　まほろ……？",
            "voice": "lesson/Lesson_30804/lesson_30804_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "まほろがカフェにいるなんて珍しいな……。\n食事中、かな？　",
            "voice": "lesson/Lesson_30804/lesson_30804_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "あ、写真撮ってる。",
            "voice": "lesson/Lesson_30804/lesson_30804_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "外から丸見えなんだけど……。\n自撮りしてるとこ見られるの、恥ずかしくないのかな？",
            "voice": "lesson/Lesson_30804/lesson_30804_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "あ、撮り直してる。\n写り悪かったのかな。",
            "voice": "lesson/Lesson_30804/lesson_30804_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "SNSだよね……、きっと。\nまほろのアカウント、見てみよう……。",
            "voice": "lesson/Lesson_30804/lesson_30804_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "わ、本当に上がってた。\n投稿時間、1分前だって。",
            "voice": "lesson/Lesson_30804/lesson_30804_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "あれ？　ここ、微妙に私の脚、写ってる。\n気付かないのかな？",
            "voice": "lesson/Lesson_30804/lesson_30804_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "でも、ただのアボカドサラダなのに、\nここまで映えさせるの、さすがまほろ……。",
            "voice": "lesson/Lesson_30804/lesson_30804_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "あれ……？　\nどこからか、ピアノの音……。",
            "voice": "lesson/Lesson_30804/lesson_30804_016.mp3"
        }
    ]
}