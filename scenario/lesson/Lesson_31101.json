{
    "Txt_Name": "Lesson_31101",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "じゃあね、あいり。あたし、あっち行くから。\nあとで合流しよ？",
            "voice": "lesson/Lesson_31101/lesson_31101_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "うん。\nあいり、悠希ちゃんにおすすめのプチキュア選んどくね。",
            "voice": "lesson/Lesson_31101/lesson_31101_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "おっけー。じゃあ、あたしもおすすめの1個、選んどくよ。",
            "voice": "lesson/Lesson_31101/lesson_31101_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "はあ、やっぱ、ダイバリオンはいいねえ。",
            "voice": "lesson/Lesson_31101/lesson_31101_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "あいりにも、そろそろダイバリオンの良さが、\n伝わってる頃だと思うんだよな。",
            "voice": "lesson/Lesson_31101/lesson_31101_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "何買おうかなあ。\nあとあいりに1個、選ぶとしたら、どうしようかな……。",
            "voice": "lesson/Lesson_31101/lesson_31101_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "あ！　あれ、もしかして……、やっぱり！",
            "voice": "lesson/Lesson_31101/lesson_31101_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "ダイバリオン・オルタ、ナハト・ムジーク・コーティング仕様、\nカトウ・キジハモデルの1/100マイスターグレードじゃんか！",
            "voice": "lesson/Lesson_31101/lesson_31101_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "限定だったからもう買えないと思ってたのに！\nしかも残り1個って。",
            "voice": "lesson/Lesson_31101/lesson_31101_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "ああ、もう。これ、完全に運命じゃん。\nこんなの買うしかないよ……。",
            "voice": "lesson/Lesson_31101/lesson_31101_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "あ、でも、これ確か、ちょっとお高いんだよなあ。\nうん、やっぱり、結構な値段……。",
            "voice": "lesson/Lesson_31101/lesson_31101_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "プチキュア、約束しちゃったから買わなきゃだし。",
            "voice": "lesson/Lesson_31101/lesson_31101_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "ああ、今月の小遣い、ヤバいんだよな……。\nどうしようかな……。",
            "voice": "lesson/Lesson_31101/lesson_31101_013.mp3"
        }
    ]
}