{
    "Txt_Name": "Lesson_20309",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "ここのカレーは、辛さが選べるみたいだよ。",
            "voice": "lesson/Lesson_20309/lesson_20309_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "1番辛くないやつ、選んだらどうかな。",
            "voice": "lesson/Lesson_20309/lesson_20309_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "……でも。",
            "voice": "lesson/Lesson_20309/lesson_20309_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "これとか、いいんじゃない？　\n甘口だって！",
            "voice": "lesson/Lesson_20309/lesson_20309_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "もし辛くてダメそうだったら、\n私の料理と交換するよ。",
            "voice": "lesson/Lesson_20309/lesson_20309_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "……じゃあ、それ、頼もうかな。",
            "voice": "lesson/Lesson_20309/lesson_20309_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "……どう？",
            "voice": "lesson/Lesson_20309/lesson_20309_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "……辛く、ない。\nおいしい……。",
            "voice": "lesson/Lesson_20309/lesson_20309_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "よかった。",
            "voice": "lesson/Lesson_20309/lesson_20309_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "ねえねえ。これも、すごくおいしいよ！　\n一口、食べてみない？",
            "voice": "lesson/Lesson_20309/lesson_20309_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "いいの？",
            "voice": "lesson/Lesson_20309/lesson_20309_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "いいのいいの！　ほら、あーん。",
            "voice": "lesson/Lesson_20309/lesson_20309_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "あーん……、―――！！？",
            "voice": "lesson/Lesson_20309/lesson_20309_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "絢！？　お、お水！　これ、お水！",
            "voice": "lesson/Lesson_20309/lesson_20309_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あ。ごめん。そんなに辛かった？",
            "voice": "lesson/Lesson_20309/lesson_20309_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "何を頼んだの！？",
            "voice": "lesson/Lesson_20309/lesson_20309_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "トムヤムクン！ ",
            "voice": "lesson/Lesson_20309/lesson_20309_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "それ、すごく辛いやつ！",
            "voice": "lesson/Lesson_20309/lesson_20309_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "……後は、お願い。",
            "voice": "lesson/Lesson_20309/lesson_20309_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "絢ーー！！",
            "voice": "lesson/Lesson_20309/lesson_20309_020.mp3"
        }
    ]
}