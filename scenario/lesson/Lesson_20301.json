{
    "Txt_Name": "Lesson_20301",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "（あと5分……。あと5分しかない。急がないとっ！）",
            "voice": "lesson/Lesson_20301/lesson_20301_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "（凛音につられて、思わず食べ過ぎちゃった……。\n　朝からフレンチトーストなんて食べるべきじゃなかったっ！）",
            "voice": "lesson/Lesson_20301/lesson_20301_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "（想像以上に時間を使っちゃった。\n　やばい、このままじゃ遅刻だ。店長に怒られる……！）",
            "voice": "lesson/Lesson_20301/lesson_20301_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "（走らないとバイトに遅刻……。）",
            "voice": "lesson/Lesson_20301/lesson_20301_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "（だがしかし、我輩が全力で街を疾走しては、\n　我輩のイメージが崩壊してしまう。）",
            "voice": "lesson/Lesson_20301/lesson_20301_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "（全力で走る我輩をもし知り合いが見たらどう思うだろう？　\n　常にクールな我輩が顔を真っ赤にして走っている姿を見たら……。）",
            "voice": "lesson/Lesson_20301/lesson_20301_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "（想像するだけで恐ろしい……。\n　今はとにかく走らず最速で、歩く！）",
            "voice": "lesson/Lesson_20301/lesson_20301_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                0
            ],
            "name": "男の子",
            "message": "お母さーん。あのお姉ちゃん変な歩き方してる～。",
            "voice": ""
        },
        {
            "index": 9,
            "heroineId": [
                0
            ],
            "name": "男の子の母親",
            "message": "見ちゃいけませんよ。",
            "voice": ""
        },
        {
            "index": 10,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "（ナチュラルに歩けば、大丈夫。\n　落ち着いて……、でも焦って……。）",
            "voice": "lesson/Lesson_20301/lesson_20301_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "あれ、利恵？",
            "voice": "lesson/Lesson_20301/lesson_20301_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "千紗じゃない。",
            "voice": "lesson/Lesson_20301/lesson_20301_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "もしかして、バイト？",
            "voice": "lesson/Lesson_20301/lesson_20301_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "そう！　千紗も？",
            "voice": "lesson/Lesson_20301/lesson_20301_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "私もこれからなの。頑張ってね！",
            "voice": "lesson/Lesson_20301/lesson_20301_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "ありがとう、千紗も頑張って！",
            "voice": "lesson/Lesson_20301/lesson_20301_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "よし！　もうひと踏ん張り！",
            "voice": "lesson/Lesson_20301/lesson_20301_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "間に合った～！！",
            "voice": "lesson/Lesson_20301/lesson_20301_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                0
            ],
            "name": "店長",
            "message": "やっときた！\n利恵ちゃん、早く着替えて。",
            "voice": ""
        },
        {
            "index": 20,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "はいっ！",
            "voice": "lesson/Lesson_20301/lesson_20301_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "今日もよく働いた……。\n帰ったらヤクロトを飲んで、",
            "voice": "lesson/Lesson_20301/lesson_20301_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "あ、利恵。バイト終わったの？",
            "voice": "lesson/Lesson_20301/lesson_20301_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "千紗？",
            "voice": "lesson/Lesson_20301/lesson_20301_023.mp3"
        }
    ]
}