{
    "Txt_Name": "Lesson_31111",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "ハロー。　",
            "voice": "lesson/Lesson_31111/lesson_31111_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "うん。大丈夫よ。\n昨日の夜も話したじゃない。",
            "voice": "lesson/Lesson_31111/lesson_31111_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "こっちはもういつもどおりだから。\nそ。今もみんな一緒。",
            "voice": "lesson/Lesson_31111/lesson_31111_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "ユーキ？　もちろんいるわよ。\n替わる？　あ、いいの？　そう。",
            "voice": "lesson/Lesson_31111/lesson_31111_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "ふふっ。オーケー。伝えとく。\nじゃ、またね。",
            "voice": "lesson/Lesson_31111/lesson_31111_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "え？　なんで今あたしの名前が出たの？　",
            "voice": "lesson/Lesson_31111/lesson_31111_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "パパが、ありがとうだって。",
            "voice": "lesson/Lesson_31111/lesson_31111_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "なんだよ、昨日もあれだけ電話で話したんだから、\n直接言ってくれればいいのに。",
            "voice": "lesson/Lesson_31111/lesson_31111_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "ふふっ。\nきっと恥ずかしいのよ。",
            "voice": "lesson/Lesson_31111/lesson_31111_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "そうなの？　",
            "voice": "lesson/Lesson_31111/lesson_31111_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "あと、みんなにも、もちろん。\n感謝していたわ。",
            "voice": "lesson/Lesson_31111/lesson_31111_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "まあ、なかなか出来ない体験をさせてもらったもんね。",
            "voice": "lesson/Lesson_31111/lesson_31111_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "えへへ。",
            "voice": "lesson/Lesson_31111/lesson_31111_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "あ、私からも、まだちゃんとお礼言ってなかったかも。\nみんな、ありがとう。",
            "voice": "lesson/Lesson_31111/lesson_31111_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "私、とっても嬉しかった！　",
            "voice": "lesson/Lesson_31111/lesson_31111_015.mp3"
        }
    ]
}