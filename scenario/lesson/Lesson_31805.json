{
    "Txt_Name": "Lesson_31805",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "(ゲームとして面白くなくなるって、\nわかってはいるけど……。)",
            "voice": "lesson/Lesson_31805/lesson_31805_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "(悪い癖かしら。\nどうしてもつい、メタな視線で状況を観察しちゃうわね。)",
            "voice": "lesson/Lesson_31805/lesson_31805_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "(でも……。\nへえ、やっぱりみんな役者ね。)",
            "voice": "lesson/Lesson_31805/lesson_31805_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "(凛音も……。うん。\nちゃんと役になれてるじゃない。)",
            "voice": "lesson/Lesson_31805/lesson_31805_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "(問題は役になりきったまま、推理と騙し合いをやりきれるか。\nそれはどうかしら。)",
            "voice": "lesson/Lesson_31805/lesson_31805_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "(少しかまをかけてみる？)",
            "voice": "lesson/Lesson_31805/lesson_31805_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "(うん。そうね。\nまずは軽く……。)",
            "voice": "lesson/Lesson_31805/lesson_31805_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "ねえ、利恵。",
            "voice": "lesson/Lesson_31805/lesson_31805_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "あ、ああ。な、なに？",
            "voice": "lesson/Lesson_31805/lesson_31805_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "あなたさっきから妙に……。\n……じゃなかった。",
            "voice": "lesson/Lesson_31805/lesson_31805_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "若宮、どうした？　さっきからどうも落ち着かないようだが。\nまるで……、なにか隠し事でもしてるような様子だな。",
            "voice": "lesson/Lesson_31805/lesson_31805_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "い、いや。そんなことはないよ嵯峨野兄さん。\n少し怖くて……。",
            "voice": "lesson/Lesson_31805/lesson_31805_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "みんなが互いに疑い合う、この状況が。\nなんだか怖いんだ。",
            "voice": "lesson/Lesson_31805/lesson_31805_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "(……これはどっち？\nあからさまに挙動不審だったけど……。)",
            "voice": "lesson/Lesson_31805/lesson_31805_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "(怖がってる演技だとしたらなかなかのものね。)",
            "voice": "lesson/Lesson_31805/lesson_31805_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                14
            ],
            "name": "宇津木聡里",
            "message": "(利恵、やるじゃない……。)",
            "voice": "lesson/Lesson_31805/lesson_31805_016.mp3"
        }
    ]
}