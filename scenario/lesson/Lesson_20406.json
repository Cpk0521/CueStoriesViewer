{
    "Txt_Name": "Lesson_20406",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "ありがとう。本当に、助かったわ。",
            "voice": "lesson/Lesson_20406/lesson_20406_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "ううん、どういたしまして。",
            "voice": "lesson/Lesson_20406/lesson_20406_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "……頼まれてたやつ、買ってきたけど。",
            "voice": "lesson/Lesson_20406/lesson_20406_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "本当……！？",
            "voice": "lesson/Lesson_20406/lesson_20406_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "「贅沢スイーツドリンク　チョコフレーバー　アップルパイ添え」\n……って。",
            "voice": "lesson/Lesson_20406/lesson_20406_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "何のお菓子かと思えば、お酒じゃん！",
            "voice": "lesson/Lesson_20406/lesson_20406_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "チョコなのか、アップルパイなのか、よくわかんないし！",
            "voice": "lesson/Lesson_20406/lesson_20406_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "そう！　新発売で、限定のお酒なの。",
            "voice": "lesson/Lesson_20406/lesson_20406_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "すごく楽しみにしてて。\nみんなで飲みたいな……、って。",
            "voice": "lesson/Lesson_20406/lesson_20406_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "ああ、だから……。\nうん？　みんな？　今夜？",
            "voice": "lesson/Lesson_20406/lesson_20406_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "……ごめん。\n明日早いから、お酒は……、ちょっと。",
            "voice": "lesson/Lesson_20406/lesson_20406_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "えっ。\n……そっか。それなら、仕方ないね。",
            "voice": "lesson/Lesson_20406/lesson_20406_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あと……。その、言いにくいんだけど。",
            "voice": "lesson/Lesson_20406/lesson_20406_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "まほろと凛音……、今日、帰り遅かった……、はず。",
            "voice": "lesson/Lesson_20406/lesson_20406_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "だから、お酒は……。",
            "voice": "lesson/Lesson_20406/lesson_20406_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "……そうなんだ。\n……いいのよ、別に。",
            "voice": "lesson/Lesson_20406/lesson_20406_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "お酒は1人でも楽しめるもんねー。",
            "voice": "lesson/Lesson_20406/lesson_20406_018.mp3"
        }
    ]
}