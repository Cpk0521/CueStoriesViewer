{
    "Txt_Name": "Lesson_31107",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                0
            ],
            "name": "警備員",
            "message": "こら、ちょっと、待ちなさい！",
            "voice": ""
        },
        {
            "index": 2,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "ごめんなさい！　待ちません！",
            "voice": "lesson/Lesson_31107/lesson_31107_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "（悠希ちゃんなら、きっとこうすると思うから。）",
            "voice": "lesson/Lesson_31107/lesson_31107_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "はあ、はあ。",
            "voice": "lesson/Lesson_31107/lesson_31107_005.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                0
            ],
            "name": "警備員",
            "message": "こら、お嬢ちゃん。そんなに走ると危ないから、\n止まりなさい。ね？",
            "voice": ""
        },
        {
            "index": 6,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "（だいぶ下りてきたけど、もう、これで大丈夫かな？\n悠希ちゃんたち、どうなったかな？）",
            "voice": "lesson/Lesson_31107/lesson_31107_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "わ！　わわ！",
            "voice": "lesson/Lesson_31107/lesson_31107_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                0
            ],
            "name": "警備員",
            "message": "あ、危ない！",
            "voice": ""
        },
        {
            "index": 9,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "はぁ、はぁ……。良かった、転ばなくて……。",
            "voice": "lesson/Lesson_31107/lesson_31107_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                0
            ],
            "name": "警備員",
            "message": "もう、そろそろ追いかけっこは終わりでいいかな？\nお嬢ちゃん。ちょっとお話、聞かせてくれる？",
            "voice": ""
        },
        {
            "index": 11,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "（うう……、まだだ……。悠希ちゃんと千紗ちゃん、\nまだ時間必要かも。）",
            "voice": "lesson/Lesson_31107/lesson_31107_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "うん。\nもう少し……、もう少し……。",
            "voice": "lesson/Lesson_31107/lesson_31107_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                0
            ],
            "name": "警備員",
            "message": "こら、待ちなさい！　ね？\nお話聞かせてもらうだけだから！",
            "voice": ""
        },
        {
            "index": 14,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "ごめんなさい！　待ちません！！",
            "voice": "lesson/Lesson_31107/lesson_31107_014.mp3"
        }
    ]
}