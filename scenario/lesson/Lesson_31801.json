{
    "Txt_Name": "Lesson_31801",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "(えー。\nこの役、もしかして、我輩、犯人役なの……？)",
            "voice": "lesson/Lesson_31801/lesson_31801_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "(同じ孤児として拾われたと思っていた一番弟子が実の兄とわかり、\n実の兄を後継者にするために育ての親を……。)",
            "voice": "lesson/Lesson_31801/lesson_31801_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "(って、え？　嘘でしょ？)",
            "voice": "lesson/Lesson_31801/lesson_31801_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "(酷すぎるよ。そんなの。)",
            "voice": "lesson/Lesson_31801/lesson_31801_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "(だって育ての親でしょ？)",
            "voice": "lesson/Lesson_31801/lesson_31801_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "(え、なんで？)",
            "voice": "lesson/Lesson_31801/lesson_31801_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "(実の兄を後継者にするためにって……。)",
            "voice": "lesson/Lesson_31801/lesson_31801_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "(後継者を指名する前に口封じすれば、\n一番弟子の実の兄が後継者になるって、そんな理由？)",
            "voice": "lesson/Lesson_31801/lesson_31801_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "(ええ……。\n他に方法なかったの？)",
            "voice": "lesson/Lesson_31801/lesson_31801_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "(あるでしょ。もっと…。もっといい方法が！)",
            "voice": "lesson/Lesson_31801/lesson_31801_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "(ああ……。犯人ってどうしてすぐに毒とか盛るのかなあ……。)",
            "voice": "lesson/Lesson_31801/lesson_31801_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                13
            ],
            "name": "丸山利恵",
            "message": "(うーん……。\n我輩、どんな気持ちでこの役に向かえばいいんだろう……？)",
            "voice": "lesson/Lesson_31801/lesson_31801_012.mp3"
        }
    ]
}