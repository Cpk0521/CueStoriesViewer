{
    "Txt_Name": "Lesson_10103",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "嬉しいんでしょ？",
            "voice": "lesson/Lesson_10103/lesson_10103_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "主役貰えて、嬉しかったんでしょ？",
            "voice": "lesson/Lesson_10103/lesson_10103_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "……。",
            "voice": "lesson/Lesson_10103/lesson_10103_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "ホントの所、どうなんだ？",
            "voice": "lesson/Lesson_10103/lesson_10103_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "すごく嬉しかった……。",
            "voice": "lesson/Lesson_10103/lesson_10103_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "やはりな。",
            "voice": "lesson/Lesson_10103/lesson_10103_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "カツカレー大盛りとパフェだもんねー。",
            "voice": "lesson/Lesson_10103/lesson_10103_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "いつもの陽菜は、\nあんな注文しないもん。",
            "voice": "lesson/Lesson_10103/lesson_10103_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "バレバレ。",
            "voice": "lesson/Lesson_10103/lesson_10103_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "……。",
            "voice": "lesson/Lesson_10103/lesson_10103_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "なんで素直に、\n嬉しいって言わなかったの？",
            "voice": "lesson/Lesson_10103/lesson_10103_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "喜んじゃいけないのかなって思って……。",
            "voice": "lesson/Lesson_10103/lesson_10103_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "なんで？",
            "voice": "lesson/Lesson_10103/lesson_10103_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "主役は、\nきっとみんなもやりたかったと思ったから……。",
            "voice": "lesson/Lesson_10103/lesson_10103_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "みんなの気持ちを考えたら、\n浮かれちゃいけないんじゃないかなって……。",
            "voice": "lesson/Lesson_10103/lesson_10103_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "バカだな……。\n気にしすぎなんだよ、陽菜は。",
            "voice": "lesson/Lesson_10103/lesson_10103_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "逆にやりにくい。",
            "voice": "lesson/Lesson_10103/lesson_10103_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "でも……。",
            "voice": "lesson/Lesson_10103/lesson_10103_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "確かに、主役になれなかったのは悔しかったけど……。",
            "voice": "lesson/Lesson_10103/lesson_10103_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "悔しいのは陽菜に対してじゃなくて……。\n実力をアピール仕切れなかった、自分に対してだし。",
            "voice": "lesson/Lesson_10103/lesson_10103_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "陽菜が気にする事じゃ無いよ。",
            "voice": "lesson/Lesson_10103/lesson_10103_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "ありがとう……。",
            "voice": "lesson/Lesson_10103/lesson_10103_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "主役が暗いと、周りもやりにくい。",
            "voice": "lesson/Lesson_10103/lesson_10103_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "そうそう。\nわたし達を盛り上げるくらいでいてよ。",
            "voice": "lesson/Lesson_10103/lesson_10103_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "が、頑張る……。",
            "voice": "lesson/Lesson_10103/lesson_10103_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "本当に？\nじゃあ改めて聞くけど、陽菜さん。",
            "voice": "lesson/Lesson_10103/lesson_10103_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "大崎あかね役に選ばれて、嬉しいですか！？",
            "voice": "lesson/Lesson_10103/lesson_10103_027.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "嬉しいです……。",
            "voice": "lesson/Lesson_10103/lesson_10103_028.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "聞こえないぞ？",
            "voice": "lesson/Lesson_10103/lesson_10103_029.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "そんな弱気でどうすんの～？",
            "voice": "lesson/Lesson_10103/lesson_10103_030.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "もう一度聞きます！　\n陽菜さん、大崎あかね役になって、嬉しいですか！？",
            "voice": "lesson/Lesson_10103/lesson_10103_031.mp3"
        },
        {
            "index": 32,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "う……、嬉しいです！",
            "voice": "lesson/Lesson_10103/lesson_10103_032.mp3"
        },
        {
            "index": 33,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "それくらい声が出れば安心だね。",
            "voice": "lesson/Lesson_10103/lesson_10103_033.mp3"
        },
        {
            "index": 34,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "見た事ないくらい、最っ高の大崎あかね。\n……演じてくれる？",
            "voice": "lesson/Lesson_10103/lesson_10103_034.mp3"
        },
        {
            "index": 35,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "わたし、最高の大崎あかねに、なってみせるよ！",
            "voice": "lesson/Lesson_10103/lesson_10103_035.mp3"
        }
    ]
}