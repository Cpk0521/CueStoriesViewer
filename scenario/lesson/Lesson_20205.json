{
    "Txt_Name": "Lesson_20205",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "ところで……。折りたたみ傘、見なかった？",
            "voice": "lesson/Lesson_20205/lesson_20205_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "明日は雨らしいから、持って行きたいんだけど……。",
            "voice": "lesson/Lesson_20205/lesson_20205_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "なぜか、見当たらなくて……。",
            "voice": "lesson/Lesson_20205/lesson_20205_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "傘？　見てないなー。",
            "voice": "lesson/Lesson_20205/lesson_20205_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "ドライスーツ着ていけば、雨にぬれても寒くないよ。",
            "voice": "lesson/Lesson_20205/lesson_20205_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "そ、それはちょっと。",
            "voice": "lesson/Lesson_20205/lesson_20205_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "だよね……。",
            "voice": "lesson/Lesson_20205/lesson_20205_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "……優しい手つき。\nすごく、丁寧に扱うんだね。",
            "voice": "lesson/Lesson_20205/lesson_20205_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "スーツ？　扱い方、難しいからね。",
            "voice": "lesson/Lesson_20205/lesson_20205_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "折りたたんだらダメだし、\n普通の服と一緒にしまってもダメだし。",
            "voice": "lesson/Lesson_20205/lesson_20205_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "でも、その分、すごく愛着が湧くんだ。",
            "voice": "lesson/Lesson_20205/lesson_20205_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "……本当に、好きなんだね。",
            "voice": "lesson/Lesson_20205/lesson_20205_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "うん。大好き。",
            "voice": "lesson/Lesson_20205/lesson_20205_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "だって、海の上を歩けるんだよ。",
            "voice": "lesson/Lesson_20205/lesson_20205_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "ビーチで眺めるのとは、全然景色がちがうの。\n風を感じながら、海の上で散歩ができるんだ。",
            "voice": "lesson/Lesson_20205/lesson_20205_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "なんか……。ロマンチックだね。",
            "voice": "lesson/Lesson_20205/lesson_20205_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "でしょ？　だからやめられないんだよね。",
            "voice": "lesson/Lesson_20205/lesson_20205_017.mp3"
        }
    ]
}