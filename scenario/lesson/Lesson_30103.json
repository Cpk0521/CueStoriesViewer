{
    "Txt_Name": "Lesson_30103",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "……みんな寝ちゃった。\n疲れてるんだね。　",
            "voice": "lesson/Lesson_30103/lesson_30103_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "そうだよね。\nいろいろあったもん。　",
            "voice": "lesson/Lesson_30103/lesson_30103_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "カラスを追いかけたり……。　",
            "voice": "lesson/Lesson_30103/lesson_30103_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "うふふ……。\nあのときは大変だったけど、思い出すとおもしろい。",
            "voice": "lesson/Lesson_30103/lesson_30103_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "だって、一番大事なお肉をカラスに取られるなんて……。",
            "voice": "lesson/Lesson_30103/lesson_30103_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "あのときの志穂ちゃんの顔……。\nもう最高。　",
            "voice": "lesson/Lesson_30103/lesson_30103_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "うう～ん……。\nもう食べられない……。　",
            "voice": "lesson/Lesson_30103/lesson_30103_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "うふふ……。\n夢の中でも、お肉食べてるのかなー……。　",
            "voice": "lesson/Lesson_30103/lesson_30103_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "志穂～……。\nそっちちゃんと持ってよー……。　",
            "voice": "lesson/Lesson_30103/lesson_30103_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "ほのかちゃんは、夢の中でもテント張ってるのかな？　",
            "voice": "lesson/Lesson_30103/lesson_30103_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "わかった～……。\nごめん～……。　",
            "voice": "lesson/Lesson_30103/lesson_30103_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "ほのかちゃんと夢の中で会話してる……。\nすごい奇跡……。",
            "voice": "lesson/Lesson_30103/lesson_30103_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "舞花ちゃんは、どんな夢見てるんだろう？　",
            "voice": "lesson/Lesson_30103/lesson_30103_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "……今日は、楽しかったねぇ～……。　",
            "voice": "lesson/Lesson_30103/lesson_30103_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "……うん。\n……楽しかったね。　",
            "voice": "lesson/Lesson_30103/lesson_30103_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "……またみんなで行こうね～。　",
            "voice": "lesson/Lesson_30103/lesson_30103_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "うん、絶対に行こう……。\nわたし、みんなのことが大好き。",
            "voice": "lesson/Lesson_30103/lesson_30103_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "ほのかちゃんのことが、志穂ちゃんのことが。\n……そして、舞花ちゃんのことが、大好き。",
            "voice": "lesson/Lesson_30103/lesson_30103_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "……ホントにぃ？　",
            "voice": "lesson/Lesson_30103/lesson_30103_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "うん、ホントだよ。\nだからもう1度……、あれ？",
            "voice": "lesson/Lesson_30103/lesson_30103_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "寝言と会話できるのおかしいよね……？　",
            "voice": "lesson/Lesson_30103/lesson_30103_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "バレたか。　",
            "voice": "lesson/Lesson_30103/lesson_30103_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "舞花ちゃんっ！\n……起きてたの？　",
            "voice": "lesson/Lesson_30103/lesson_30103_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "陽菜の告白、全部聞かせてもらったよー。　",
            "voice": "lesson/Lesson_30103/lesson_30103_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "恥ずかしい……。\nうぅ……、忘れて。　",
            "voice": "lesson/Lesson_30103/lesson_30103_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "ううん。絶対に忘れない。\n一生覚えてる。",
            "voice": "lesson/Lesson_30103/lesson_30103_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "……ま、いっか。\nわたしも、ずっと覚えてる。",
            "voice": "lesson/Lesson_30103/lesson_30103_027.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "絶対に、忘れない。　",
            "voice": "lesson/Lesson_30103/lesson_30103_028.mp3"
        }
    ]
}