{
    "Txt_Name": "Lesson_30801",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うん、今日もいい音。",
            "voice": "lesson/Lesson_30801/lesson_30801_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "幸せな1日。\n指先も鍵盤も、喜んでるのが伝わってくる。",
            "voice": "lesson/Lesson_30801/lesson_30801_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "やっほー！　絢。何してんの？",
            "voice": "lesson/Lesson_30801/lesson_30801_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "あら、莉子の声？\n絢もいるの？",
            "voice": "lesson/Lesson_30801/lesson_30801_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "あらあら。道でお話してる。\n2人とも、偶然会ったのかなぁ。",
            "voice": "lesson/Lesson_30801/lesson_30801_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "それもエールブルーのビルの前でなんて。\nこれは運命かも。",
            "voice": "lesson/Lesson_30801/lesson_30801_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "莉子は……、バイト中なのね。",
            "voice": "lesson/Lesson_30801/lesson_30801_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "絢は……、散歩中かしら。うふふ。",
            "voice": "lesson/Lesson_30801/lesson_30801_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "うん、またねー。",
            "voice": "lesson/Lesson_30801/lesson_30801_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "あ、行っちゃった。",
            "voice": "lesson/Lesson_30801/lesson_30801_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "不意の出会いと別れ、ね。うふふ。",
            "voice": "lesson/Lesson_30801/lesson_30801_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "あれ？　今、下のカフェから出てきたの、まほろじゃない？",
            "voice": "lesson/Lesson_30801/lesson_30801_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うん。あの髪型、きっとそう！",
            "voice": "lesson/Lesson_30801/lesson_30801_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "まほろは、絢と莉子には気付いてなかったのかなぁ。",
            "voice": "lesson/Lesson_30801/lesson_30801_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "でも……、うふふ。\nなーんだ、みんな近くにいたんじゃない。",
            "voice": "lesson/Lesson_30801/lesson_30801_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "やっぱり、これはもう運命ね。ふふ。",
            "voice": "lesson/Lesson_30801/lesson_30801_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "今日の日記に、書いておこうっと。",
            "voice": "lesson/Lesson_30801/lesson_30801_017.mp3"
        }
    ]
}