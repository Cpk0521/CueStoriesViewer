{
    "Txt_Name": "Main_01_01_21",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "……。",
            "voice": "main_01/Main_01_01/Main_01_01_21/main_01_01_21_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "はーい。",
            "voice": "main_01/Main_01_01/Main_01_01_21/main_01_01_21_004.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "ＣＩＡだ。大人しくしろ。",
            "voice": "main_01/Main_01_01/Main_01_01_21/main_01_01_21_005.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "……何の用？",
            "voice": "main_01/Main_01_01/Main_01_01_21/main_01_01_21_006.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "ノリが悪い。マネージャーを見習え。",
            "voice": "main_01/Main_01_01/Main_01_01_21/main_01_01_21_007.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "はぁ？",
            "voice": "main_01/Main_01_01/Main_01_01_21/main_01_01_21_008.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "何してた？",
            "voice": "main_01/Main_01_01/Main_01_01_21/main_01_01_21_009.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "トレーニングだけど。",
            "voice": "main_01/Main_01_01/Main_01_01_21/main_01_01_21_010.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "他にやることないのか。",
            "voice": "main_01/Main_01_01/Main_01_01_21/main_01_01_21_011.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "トレーニングしてると、いろんなことを忘れられるの。\nむしゃくしゃすることも、悲しいことも……。",
            "voice": "main_01/Main_01_01/Main_01_01_21/main_01_01_21_012.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "……。",
            "voice": "main_01/Main_01_01/Main_01_01_21/main_01_01_21_013.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "でっ？\n何の用なの？",
            "voice": "main_01/Main_01_01/Main_01_01_21/main_01_01_21_014.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "ほのかの家の犬は、どんな犬だった？",
            "voice": "main_01/Main_01_01/Main_01_01_21/main_01_01_21_015.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "えっ？",
            "voice": "main_01/Main_01_01/Main_01_01_21/main_01_01_21_016.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "教えてくれ。",
            "voice": "main_01/Main_01_01/Main_01_01_21/main_01_01_21_017.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "……ベラっていうの。とても人懐っこい子だったよ。\nいつだってわたしに元気をくれた。",
            "voice": "main_01/Main_01_01/Main_01_01_21/main_01_01_21_018.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "どんなときも？",
            "voice": "main_01/Main_01_01/Main_01_01_21/main_01_01_21_019.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "声優になるのを親に反対されて、しょぼくれていた時も、\nいつもわたしの隣にいてくれて、元気づけてくれたんだ。",
            "voice": "main_01/Main_01_01/Main_01_01_21/main_01_01_21_020.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "そうか……。",
            "voice": "main_01/Main_01_01/Main_01_01_21/main_01_01_21_021.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "あの子に、声優になったわたしの姿を見せたかった……。",
            "voice": "main_01/Main_01_01/Main_01_01_21/main_01_01_21_022.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "……。",
            "voice": "main_01/Main_01_01/Main_01_01_21/main_01_01_21_023.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "志穂、泣いてるの……？",
            "voice": "main_01/Main_01_01/Main_01_01_21/main_01_01_21_024.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "ほのかの気持ちになってみた……。",
            "voice": "main_01/Main_01_01/Main_01_01_21/main_01_01_21_025.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "……。",
            "voice": "main_01/Main_01_01/Main_01_01_21/main_01_01_21_026.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "実家の寺の手伝いしてるとき、私の声に怒った客がいた。",
            "voice": "main_01/Main_01_01/Main_01_01_21/main_01_01_21_027.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "私は、この不謹慎な声のせいだと思った。\nでも……。",
            "voice": "main_01/Main_01_01/Main_01_01_21/main_01_01_21_028.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "でも……？",
            "voice": "main_01/Main_01_01/Main_01_01_21/main_01_01_21_029.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "大事なのは声じゃない。心だ。\n心が足りなかった。",
            "voice": "main_01/Main_01_01/Main_01_01_21/main_01_01_21_030.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "心……。",
            "voice": "main_01/Main_01_01/Main_01_01_21/main_01_01_21_031.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "ほのか……、辛かったな。",
            "voice": "main_01/Main_01_01/Main_01_01_21/main_01_01_21_032.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "志穂……。",
            "voice": "main_01/Main_01_01/Main_01_01_21/main_01_01_21_033.mp3"
        },
        {
            "index": 32,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "今は、ベラのために悲しんでやるといい。",
            "voice": "main_01/Main_01_01/Main_01_01_21/main_01_01_21_034.mp3"
        },
        {
            "index": 33,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "うん……、そうだね。",
            "voice": "main_01/Main_01_01/Main_01_01_21/main_01_01_21_035.mp3"
        },
        {
            "index": 34,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "『あなたなんて、大っ嫌い！』",
            "voice": "main_01/Main_01_01/Main_01_01_21/main_01_01_21_037.mp3"
        },
        {
            "index": 35,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "『嫌いでもいい』",
            "voice": "main_01/Main_01_01/Main_01_01_21/main_01_01_21_038.mp3"
        },
        {
            "index": 36,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "『二度と顔も見たくない！』",
            "voice": "main_01/Main_01_01/Main_01_01_21/main_01_01_21_039.mp3"
        },
        {
            "index": 37,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "『それでも、僕は君に会いに行く。\n何度でも、何度でも』",
            "voice": "main_01/Main_01_01/Main_01_01_21/main_01_01_21_040.mp3"
        },
        {
            "index": 38,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "『どうしてそこまでするの？』",
            "voice": "main_01/Main_01_01/Main_01_01_21/main_01_01_21_041.mp3"
        },
        {
            "index": 39,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "『決まってるじゃないか。\n僕は君を……、愛してる』",
            "voice": "main_01/Main_01_01/Main_01_01_21/main_01_01_21_042.mp3"
        },
        {
            "index": 40,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "はい、\nそこまで。",
            "voice": "main_01/Main_01_01/Main_01_01_21/main_01_01_21_043.mp3"
        },
        {
            "index": 41,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "志穂ちゃん……。",
            "voice": "main_01/Main_01_01/Main_01_01_21/main_01_01_21_044.mp3"
        },
        {
            "index": 42,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "すごい……。",
            "voice": "main_01/Main_01_01/Main_01_01_21/main_01_01_21_045.mp3"
        },
        {
            "index": 43,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "鹿野さん。ひと皮むけたわね。\nなにかあったのかしら？",
            "voice": "main_01/Main_01_01/Main_01_01_21/main_01_01_21_046.mp3"
        },
        {
            "index": 44,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "あざーす。",
            "voice": "main_01/Main_01_01/Main_01_01_21/main_01_01_21_047.mp3"
        },
        {
            "index": 45,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "ふふふっ。",
            "voice": "main_01/Main_01_01/Main_01_01_21/main_01_01_21_048.mp3"
        },
        {
            "index": 46,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "（あれから志穂はほのかと話したのかな？\n何はともあれよかった！）",
            "voice": ""
        }
    ]
}