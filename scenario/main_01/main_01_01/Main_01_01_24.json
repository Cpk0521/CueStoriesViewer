{
    "Txt_Name": "Main_01_01_24",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "『もうダメ……、もう逃げられない……。』",
            "voice": "main_01/Main_01_01/Main_01_01_24/main_01_01_24_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "『私達、やられちゃうの……？』",
            "voice": "main_01/Main_01_01/Main_01_01_24/main_01_01_24_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "『誰か、助けて……！』",
            "voice": "main_01/Main_01_01/Main_01_01_24/main_01_01_24_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "『フッフッフ……、ここまでのようだな……。\n死ねぇええ！』",
            "voice": "main_01/Main_01_01/Main_01_01_24/main_01_01_24_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "『待つんだ！！』",
            "voice": "main_01/Main_01_01/Main_01_01_24/main_01_01_24_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "『誰だ！\n貴様は！』",
            "voice": "main_01/Main_01_01/Main_01_01_24/main_01_01_24_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "『みんなのことは、私が守る！』",
            "voice": "main_01/Main_01_01/Main_01_01_24/main_01_01_24_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "『こしゃくな……。\nお前から相手をしてやる。うりゃああ！』",
            "voice": "main_01/Main_01_01/Main_01_01_24/main_01_01_24_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "『ぐあああ！』",
            "voice": "main_01/Main_01_01/Main_01_01_24/main_01_01_24_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "『今度こそ、本当に終わりだわ……。』",
            "voice": "main_01/Main_01_01/Main_01_01_24/main_01_01_24_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "『まだ終わりじゃない……、諦めたらそこで試合終了よ！』",
            "voice": "main_01/Main_01_01/Main_01_01_24/main_01_01_24_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "『私は、みんなを助けたいの！\nみんなを助けるためなら……、命だって惜しくはない！』",
            "voice": "main_01/Main_01_01/Main_01_01_24/main_01_01_24_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "『黙れ黙れ！\n自分の命より大切なものなどあるものか！』",
            "voice": "main_01/Main_01_01/Main_01_01_24/main_01_01_24_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "『あなたにはわからないでしょうね……。』",
            "voice": "main_01/Main_01_01/Main_01_01_24/main_01_01_24_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "『死ねぇええええええ！』",
            "voice": "main_01/Main_01_01/Main_01_01_24/main_01_01_24_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "『うおりゃああああああ！』",
            "voice": "main_01/Main_01_01/Main_01_01_24/main_01_01_24_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "はい、そこまで。\nお疲れ様。",
            "voice": "main_01/Main_01_01/Main_01_01_24/main_01_01_24_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "ありがとうございました！",
            "voice": "main_01/Main_01_01/Main_01_01_24/main_01_01_24_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "（ほのか、さすがだな……。）",
            "voice": ""
        },
        {
            "index": 20,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "ほのかちゃん、熱演だったね。",
            "voice": "main_01/Main_01_01/Main_01_01_24/main_01_01_24_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "ヒーローものであいつに勝てるやつはいない。",
            "voice": "main_01/Main_01_01/Main_01_01_24/main_01_01_24_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "ていうか、桐香先生もすごくね？",
            "voice": "main_01/Main_01_01/Main_01_01_24/main_01_01_24_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "うん。\nすごい迫力だった。",
            "voice": "main_01/Main_01_01/Main_01_01_24/main_01_01_24_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "キャラが乗り移ってた。",
            "voice": "main_01/Main_01_01/Main_01_01_24/main_01_01_24_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "当然だよ。\n桐香先生はプロの声優だったんだから。",
            "voice": ""
        },
        {
            "index": 26,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "やっぱり、そうだったんだ……！",
            "voice": "main_01/Main_01_01/Main_01_01_24/main_01_01_24_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "昔の話よ。",
            "voice": "main_01/Main_01_01/Main_01_01_24/main_01_01_24_027.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "でも、どっかで先生の声を聞いたことある気がするんだよね……。",
            "voice": "main_01/Main_01_01/Main_01_01_24/main_01_01_24_028.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "どうして今はやってないんですか？",
            "voice": "main_01/Main_01_01/Main_01_01_24/main_01_01_24_029.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "そうだよ。\nもったいない。",
            "voice": "main_01/Main_01_01/Main_01_01_24/main_01_01_24_030.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "まだまだイケる。",
            "voice": "main_01/Main_01_01/Main_01_01_24/main_01_01_24_031.mp3"
        },
        {
            "index": 32,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "ありがと。でも今は、違うことにやりがいを感じてるの。\nあなた達のようなタマゴたちを育てることにね。",
            "voice": "main_01/Main_01_01/Main_01_01_24/main_01_01_24_032.mp3"
        },
        {
            "index": 33,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "そうだ……、思い出した！",
            "voice": ""
        },
        {
            "index": 34,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "何を思い出したんですか？",
            "voice": "main_01/Main_01_01/Main_01_01_24/main_01_01_24_034.mp3"
        },
        {
            "index": 35,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "昨日、話してた声優さん。\n唐木ユリって桐香先生の昔の芸名だ。",
            "voice": ""
        },
        {
            "index": 36,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "えっ！？\n唐木ユリが桐香先生の芸名！？",
            "voice": "main_01/Main_01_01/Main_01_01_24/main_01_01_24_036.mp3"
        },
        {
            "index": 37,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "そう。\n由良桐香を並べ替えて、唐木ユリって芸名にしてたの。",
            "voice": "main_01/Main_01_01/Main_01_01_24/main_01_01_24_037.mp3"
        },
        {
            "index": 38,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "じゃあ、桐香先生がほのかちゃんのライバル……。",
            "voice": "main_01/Main_01_01/Main_01_01_24/main_01_01_24_038.mp3"
        },
        {
            "index": 39,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "ライバルって何？",
            "voice": "main_01/Main_01_01/Main_01_01_24/main_01_01_24_039.mp3"
        },
        {
            "index": 40,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "な、何でもないです！",
            "voice": "main_01/Main_01_01/Main_01_01_24/main_01_01_24_040.mp3"
        },
        {
            "index": 41,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "そういえば、サンダーウーマンっていうアメコミの吹き替えやってるとき、\n小さな女の子から、ファンレター貰ったことがあるわ。",
            "voice": "main_01/Main_01_01/Main_01_01_24/main_01_01_24_041.mp3"
        },
        {
            "index": 42,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "『私の夢は、サンダーウーマンになることです』\n『いつかあなたから、サンダーウーマンを貰います』って。",
            "voice": "main_01/Main_01_01/Main_01_01_24/main_01_01_24_042.mp3"
        },
        {
            "index": 43,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "それって、もしかして……。",
            "voice": ""
        },
        {
            "index": 44,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "生意気な子供ですね……。",
            "voice": "main_01/Main_01_01/Main_01_01_24/main_01_01_24_044.mp3"
        },
        {
            "index": 45,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "でも嬉しかった。\nそれくらい、サンダーウーマンが大好きってことでしょ？",
            "voice": "main_01/Main_01_01/Main_01_01_24/main_01_01_24_045.mp3"
        },
        {
            "index": 46,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "声優をやってて良かったって思ったわ。",
            "voice": "main_01/Main_01_01/Main_01_01_24/main_01_01_24_046.mp3"
        },
        {
            "index": 47,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "桐香先生は、その子のヒーローだったんですよ。\nきっと、今でも……。",
            "voice": "main_01/Main_01_01/Main_01_01_24/main_01_01_24_047.mp3"
        },
        {
            "index": 48,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "私が引退してから、新しいサンダーウーマンはまだ決まってない。\nその子が新しいサンダーウーマンになるチャンスはあるわね。",
            "voice": "main_01/Main_01_01/Main_01_01_24/main_01_01_24_048.mp3"
        },
        {
            "index": 49,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "そうですね！",
            "voice": "main_01/Main_01_01/Main_01_01_24/main_01_01_24_049.mp3"
        },
        {
            "index": 50,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "でも、簡単にサンダーウーマンにはなれない。\n沢山レッスンして、もっともっと上手くならないとね。",
            "voice": "main_01/Main_01_01/Main_01_01_24/main_01_01_24_050.mp3"
        },
        {
            "index": 51,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "（桐香先生……、ファンレターがほのかからだって気付いてる……？）",
            "voice": ""
        },
        {
            "index": 52,
            "heroineId": [
                103
            ],
            "name": "由良桐香",
            "message": "もっともっと、頑張りなさい。",
            "voice": "main_01/Main_01_01/Main_01_01_24/main_01_01_24_052.mp3"
        },
        {
            "index": 53,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "はい……！　頑張ります！",
            "voice": "main_01/Main_01_01/Main_01_01_24/main_01_01_24_053.mp3"
        }
    ]
}