{
    "Txt_Name": "Main_01_01_07",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "コラー！\nほのか！",
            "voice": "main_01/main_01_01/Main_01_01_07/main_01_01_07_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "なになに？\nどうした？",
            "voice": "main_01/main_01_01/Main_01_01_07/main_01_01_07_003.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "ほのかでしょ！\nゴミ箱にりんご捨てたの！",
            "voice": "main_01/main_01_01/Main_01_01_07/main_01_01_07_004.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "りんごって、このりんごの芯のこと？",
            "voice": "main_01/main_01_01/Main_01_01_07/main_01_01_07_005.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "よく見て！　まだ食べれるじゃん！\nもったいない！",
            "voice": "main_01/main_01_01/Main_01_01_07/main_01_01_07_006.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "いや、これ以上は無理だよ。",
            "voice": "main_01/main_01_01/Main_01_01_07/main_01_01_07_007.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "食べれるよ！\n芯だって頑張れば食べれるのに！",
            "voice": "main_01/main_01_01/Main_01_01_07/main_01_01_07_008.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "舞花ちゃんの声が聞こえたんだけど、\n何かあった……？",
            "voice": "main_01/main_01_01/Main_01_01_07/main_01_01_07_009.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "あ、陽菜。\nちょっと何とかしてよ。",
            "voice": "main_01/main_01_01/Main_01_01_07/main_01_01_07_010.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "舞花がりんごの芯捨てたのに怒ってて……。\nなんか言ってること、メチャクチャなの。",
            "voice": "main_01/main_01_01/Main_01_01_07/main_01_01_07_011.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "メチャクチャじゃないよ！\nほんっとこういうの我慢できないの！",
            "voice": "main_01/main_01_01/Main_01_01_07/main_01_01_07_012.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "舞花ちゃん、ちょっと落ち着いて……。",
            "voice": "main_01/main_01_01/Main_01_01_07/main_01_01_07_013.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "そういえばわたしも聞きたいんだけど。\nわたしのプロテインシェイカーに花飾ったの誰。",
            "voice": "main_01/main_01_01/Main_01_01_07/main_01_01_07_014.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "私が飾った。",
            "voice": "main_01/main_01_01/Main_01_01_07/main_01_01_07_015.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "し、志穂ちゃん！？",
            "voice": "main_01/main_01_01/Main_01_01_07/main_01_01_07_016.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "花あった方がいいかなって思って。",
            "voice": "main_01/main_01_01/Main_01_01_07/main_01_01_07_017.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "だからって、\nわたしのプロテインシェイカーを花瓶にすることないじゃん！",
            "voice": "main_01/main_01_01/Main_01_01_07/main_01_01_07_018.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "いらないと思った。\nその辺にポイって転がってたし。",
            "voice": "main_01/main_01_01/Main_01_01_07/main_01_01_07_019.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "ちょっと片付け忘れただけでしょ？\nこのシェイカーは、命の次に大事なものよ！",
            "voice": "main_01/main_01_01/Main_01_01_07/main_01_01_07_020.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "命の次に大事なのに片づけ忘れるのか。",
            "voice": "main_01/main_01_01/Main_01_01_07/main_01_01_07_021.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "うるさいな！\nしかも何で菊の花を飾るの？",
            "voice": "main_01/main_01_01/Main_01_01_07/main_01_01_07_022.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "落ち着く。",
            "voice": "main_01/main_01_01/Main_01_01_07/main_01_01_07_023.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "落ち着かない！\n勝手にシェイカーを亡き者にしないで！",
            "voice": "main_01/main_01_01/Main_01_01_07/main_01_01_07_024.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "ならばこっちも問おう。\n冷蔵庫のプリンを食べたのは誰だ。",
            "voice": "main_01/main_01_01/Main_01_01_07/main_01_01_07_025.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "プリン？\nわたしは知らないけど。",
            "voice": "main_01/main_01_01/Main_01_01_07/main_01_01_07_026.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "あぁ、あれ志穂のだったんだ。",
            "voice": "main_01/main_01_01/Main_01_01_07/main_01_01_07_027.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "名前書いてないから、\nマネージャーからの差し入れかと思って食べちゃったよ。",
            "voice": "main_01/main_01_01/Main_01_01_07/main_01_01_07_028.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "志穂ちゃんの名前書いてなかったの？",
            "voice": "main_01/main_01_01/Main_01_01_07/main_01_01_07_029.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "書いてなかったよ。",
            "voice": "main_01/main_01_01/Main_01_01_07/main_01_01_07_030.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "星印が逆さに書いてあった。",
            "voice": "main_01/main_01_01/Main_01_01_07/main_01_01_07_031.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "「ほし」が逆さで「しほ」。\nそのくらい分かれ。",
            "voice": "main_01/main_01_01/Main_01_01_07/main_01_01_07_032.mp3"
        },
        {
            "index": 32,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "そんなの分かんないってー。\n名前を書くルールでしょ？",
            "voice": "main_01/main_01_01/Main_01_01_07/main_01_01_07_033.mp3"
        },
        {
            "index": 33,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "分かんないから勝手に食べるのか？",
            "voice": "main_01/main_01_01/Main_01_01_07/main_01_01_07_034.mp3"
        },
        {
            "index": 34,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "何よ、志穂が悪いからね？\n名前書いてないんだから食べられても文句言えないよ。",
            "voice": "main_01/main_01_01/Main_01_01_07/main_01_01_07_035.mp3"
        },
        {
            "index": 35,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "大体あのプリン昨日からあるじゃん。\nその日のうちに食べないとこうやって食べられるの。わかる？",
            "voice": "main_01/main_01_01/Main_01_01_07/main_01_01_07_036.mp3"
        },
        {
            "index": 36,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "舞花の家と一緒にするな。",
            "voice": "main_01/main_01_01/Main_01_01_07/main_01_01_07_037.mp3"
        },
        {
            "index": 37,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "今日食べるために昨日買った。\n食べ物の恨みは怖いぞ。",
            "voice": "main_01/main_01_01/Main_01_01_07/main_01_01_07_038.mp3"
        },
        {
            "index": 38,
            "heroineId": [
                2
            ],
            "name": "鷹取舞花",
            "message": "なに？　やるの？\n力なら負けないよ。",
            "voice": "main_01/main_01_01/Main_01_01_07/main_01_01_07_039.mp3"
        },
        {
            "index": 39,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "ちょっと皆……。\nケンカはやめて……。",
            "voice": "main_01/main_01_01/Main_01_01_07/main_01_01_07_040.mp3"
        },
        {
            "index": 40,
            "heroineId": [
                4
            ],
            "name": "月居ほのか",
            "message": "2人だけでケンカしないでよ！\nわたしのプロテインシェイカーだって被害者なんだからね！",
            "voice": "main_01/main_01_01/Main_01_01_07/main_01_01_07_041.mp3"
        },
        {
            "index": 41,
            "heroineId": [
                1
            ],
            "name": "六石陽菜",
            "message": "ほのかちゃん！？",
            "voice": "main_01/main_01_01/Main_01_01_07/main_01_01_07_042.mp3"
        },
        {
            "index": 42,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "あ、陽菜。それに皆？\nちょうどよかった。",
            "voice": ""
        },
        {
            "index": 43,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "社長が呼んでるから、\n明日は皆で事務所に来て。",
            "voice": ""
        }
    ]
}