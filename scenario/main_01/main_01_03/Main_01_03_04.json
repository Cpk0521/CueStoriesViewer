{
    "Txt_Name": "Main_01_03_04",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "……ねえ、莉子。",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "おはよう、まほろ。\nどうしたの？",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "あんた昨日……、お風呂入ったよね？",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "昨日？　いやいや、ちゃんと毎日入ってるよ！\nえっ？　なに？　あたし、汗臭い？",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "違う、そうじゃなくて……。\nお風呂、1時間くらい入ってたでしょ？",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "昨日の夜は……。うん、そうね。\nたしかに1時間……、入ってたかな。",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "お風呂は1人20分までってルール、知ってる？",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "知ってるよー。",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "おや……？",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "んっ？　なに？\nどうかした？",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "1時間入ってしまったことについて、ほかに言うことないわけ？",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "んー？　どういうこと？　一緒に入ってくれば良かったじゃん。\n共同のお風呂なんだから。",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "嫌よ。\n1人で入りたいの。",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "じゃあ、部屋のシャワー使ったら？",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "湯船に浸かりたい。",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "……あたしが出た後って、誰か他の人入ってた？",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "知らない。\nすぐシャワー浴びて寝たから。",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "……。",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "……。",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あははっ。\nじゃ、何も問題ないじゃーん。",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_000.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あー、びっくりしたー。\nあたし、寮のみんなになんかすっごい迷惑かけちゃったのかと思った。",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_000.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "問題ありありでしょ。\n何言ってんの。",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_021.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "あ、莉子、まほろ。\nおはよう。",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_022.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "おー、絢いいとこに来てくれた。",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_023.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "いやー、今ね。まほろにあたしのお風呂の入り方について、\nダメ出しされちゃってるとこでさー。",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_024.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "絢はあたしのお風呂の入り方って、どう思う？",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_025.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "……烏の行水？",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_026.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "えっ？",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_027.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "早すぎないかって思ってる。\nちゃんと洗えてるかって心配になる。",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_028.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "やだ！　ちょっと！　絢までそういうこと言うわけ？\nえっ？　もしかしてあたし、本当に汗臭い？",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_029.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "いやいやいや？　逆だよね？\n長風呂でしょ？",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_030.mp3"
        },
        {
            "index": 32,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "いつも……、10分くらいだよね？",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_031.mp3"
        },
        {
            "index": 33,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "全行程で15分くらいかな。",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_032.mp3"
        },
        {
            "index": 34,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "もっとゆっくり使えばいいのに。",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_033.mp3"
        },
        {
            "index": 35,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "そうなんだけどねー。\n子供の頃からのクセでねー。",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_034.mp3"
        },
        {
            "index": 36,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ちゃんと洗えてるならいいけど……。",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_035.mp3"
        },
        {
            "index": 37,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "えっ？　ちょっと待ってよ。\n昨日、1時間入ってたって、さっき自分で認めてたじゃない。",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_036.mp3"
        },
        {
            "index": 38,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "そうね。だいたい週１くらいでそういう時があるね！\nゆっくり入るお風呂ってのも、たまにはいいよねー。",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_037.mp3"
        },
        {
            "index": 39,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "はぁ？　何言ってるの？　なんで悪びれないの？　\nえっ？　もしかしてまほろが変なの？",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_038.mp3"
        },
        {
            "index": 40,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "みんなー、おはよー。\nあれれ？　どうかしたの？　",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_039.mp3"
        },
        {
            "index": 41,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "あ、美晴。ちょっと聞いてよ。\n莉子が、昨日1時間もお風呂入ってて……。",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_040.mp3"
        },
        {
            "index": 42,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "知ってるよ。\nわたしも一緒に入ってたから。",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_041.mp3"
        },
        {
            "index": 43,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "えっ！？　美晴も？\n1時間？",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_042.mp3"
        },
        {
            "index": 44,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "わたしはその前から入ってたから……、2時間！\nてへ。",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_043.mp3"
        },
        {
            "index": 45,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ちょっとちょっと。\n入浴時間20分のルールって、どうなってるの？",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_044.mp3"
        },
        {
            "index": 46,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "あったような……、なかったような……？",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_045.mp3"
        },
        {
            "index": 47,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "その程度の認識なわけ！？",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_046.mp3"
        },
        {
            "index": 48,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "まほろこそさ、なんでそんなにルールにこだわるの？",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_047.mp3"
        },
        {
            "index": 49,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "こだわるっていうか、えっ？\nそういうことじゃなくない？",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_048.mp3"
        },
        {
            "index": 50,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "時間が被っちゃうのは仕方ないと思う。\nでも、20分待てば自分の時間が来るって普通そう思うでしょ。",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_049.mp3"
        },
        {
            "index": 51,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "だから……、まほろも一緒に入ればよかったじゃん。",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_050.mp3"
        },
        {
            "index": 52,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うんうん。\nみんなで入りましょー。",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_051.mp3"
        },
        {
            "index": 53,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "……。",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_052.mp3"
        },
        {
            "index": 54,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "確かに共同のお風呂だけど、\n基本的にお風呂って1人で入るものじゃん。",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_053.mp3"
        },
        {
            "index": 55,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "そうなの？",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_054.mp3"
        },
        {
            "index": 56,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うふふ。\nまほろは1人で入るタイプなのねー。",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_055.mp3"
        },
        {
            "index": 57,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "あー、もうヤダ！\nわかってたけど、まほろ、共同生活とか向いてないんだよね。",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_056.mp3"
        },
        {
            "index": 58,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "……まほろ？\nちょっと、言わせてほしいんだけど。",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_057.mp3"
        },
        {
            "index": 59,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "なによ……？",
            "voice": "main_01/Main_01_03/Main_01_03_04/main_01_03_04_058.mp3"
        }
    ]
}