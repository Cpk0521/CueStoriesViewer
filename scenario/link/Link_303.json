{
    "Txt_Name": "Link_303",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ねむい……。　",
            "voice": "link/Link_303/link_303_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "なぜ、朝は来るのか……。",
            "voice": "link/Link_303/link_303_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "もう、ずっと夜でお願い……。\n志穂、おでこ、赤くない？",
            "voice": "link/Link_303/link_303_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "おでこ？　",
            "voice": "link/Link_303/link_303_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ぶつけたとか？　",
            "voice": "link/Link_303/link_303_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "そういう記憶はない。　",
            "voice": "link/Link_303/link_303_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "……ごはん。　",
            "voice": "link/Link_303/link_303_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "鳴。朝食なら、そこに……、ない。　",
            "voice": "link/Link_303/link_303_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "あー、ごめん。今日の当番、まほろだ……。\n眠すぎて、忘れてた……。",
            "voice": "link/Link_303/link_303_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "昨日の残りがあったはず……。\nはい、お茶碗。",
            "voice": "link/Link_303/link_303_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "それ、コップ。　",
            "voice": "link/Link_303/link_303_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "あれ？　",
            "voice": "link/Link_303/link_303_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "……痛い。　",
            "voice": "link/Link_303/link_303_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "見事。綺麗に壁にぶつかった。　",
            "voice": "link/Link_303/link_303_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "もしかして……、志穂も、ぶつけたんじゃない？\nあんな風に。",
            "voice": "link/Link_303/link_303_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "確かに……。\n寝ぼけていたから、記憶に残っていないだけか……。",
            "voice": "link/Link_303/link_303_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "お見事。綺麗にコケた。　",
            "voice": "link/Link_303/link_303_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                16
            ],
            "name": "遠見鳴",
            "message": "……ぐう。　",
            "voice": "link/Link_303/link_303_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "そして寝た。　",
            "voice": "link/Link_303/link_303_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "いいなー。まほろも寝たい。\n……寝ちゃおうかな。",
            "voice": "link/Link_303/link_303_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "床、気持ちいいー。\n志穂も来なよー。みんなで寝よう。",
            "voice": "link/Link_303/link_303_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "学校……。　",
            "voice": "link/Link_303/link_303_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "まあ、いいか。　",
            "voice": "link/Link_303/link_303_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "みんなでお昼寝だー。　",
            "voice": "link/Link_303/link_303_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                3
            ],
            "name": "鹿野志穂",
            "message": "ふあぁ。至福……。　",
            "voice": "link/Link_303/link_303_025.mp3"
        }
    ]
}