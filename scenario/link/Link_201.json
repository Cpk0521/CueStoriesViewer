{
    "Txt_Name": "Link_201",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "可愛い。",
            "voice": "link/Link_201/link_201_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "うん。可愛い。",
            "voice": "link/Link_201/link_201_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "何やってるの？\n2人揃って、本とにらめっこして。",
            "voice": "link/Link_201/link_201_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "なんかー、鳥を飼いたいんだって。　",
            "voice": "link/Link_201/link_201_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "鳥？　",
            "voice": "link/Link_201/link_201_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "チサ！\nこの子欲しい！",
            "voice": "link/Link_201/link_201_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "なに？　シマエナガ？\nずいぶんと、白くて丸い鳥ね。",
            "voice": "link/Link_201/link_201_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "雪の妖精って言われてるみたい。　",
            "voice": "link/Link_201/link_201_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "妖精？　なんか、大福みたいだけど。\nおいしそー。",
            "voice": "link/Link_201/link_201_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "食べ物じゃないもん！　",
            "voice": "link/Link_201/link_201_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "この可愛らしいボディ、真っ白な毛皮。\nそして何より……、鳥！",
            "voice": "link/Link_201/link_201_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "Birdの私たちにぴったりじゃない？\nチサ、この子飼いましょう！",
            "voice": "link/Link_201/link_201_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "ダメ。　",
            "voice": "link/Link_201/link_201_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "えぇ……。　",
            "voice": "link/Link_201/link_201_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "そもそも。\nシマエナガって、飼育できるものなの？",
            "voice": "link/Link_201/link_201_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "飼えたとして、誰が世話をするの？\nみんな忙しいし、現実的じゃないわ。",
            "voice": "link/Link_201/link_201_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "こんなに可愛いのに……。　",
            "voice": "link/Link_201/link_201_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                5
            ],
            "name": "天童悠希",
            "message": "あたしは鳥より、鶏肉の方が好きだなー。　",
            "voice": "link/Link_201/link_201_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "だから、食べ物じゃないって！　",
            "voice": "link/Link_201/link_201_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "生き物を飼うっていうのは、とても重いことなのよ。\nもっとじっくり考えないと。",
            "voice": "link/Link_201/link_201_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "……わかったわ。　",
            "voice": "link/Link_201/link_201_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                7
            ],
            "name": "恵庭あいり",
            "message": "柚葉ちゃん……。　",
            "voice": "link/Link_201/link_201_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "我慢するわ。\n3日だけね！",
            "voice": "link/Link_201/link_201_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "みじかっ！　",
            "voice": "link/Link_201/link_201_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                8
            ],
            "name": "九条柚葉",
            "message": "3日後を楽しみにしてなさい、チサ。\n嫌になるぐらい、プレゼンしてあげるんだから。",
            "voice": "link/Link_201/link_201_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                6
            ],
            "name": "赤川千紗",
            "message": "3日後、大変になりそうね……。　",
            "voice": "link/Link_201/link_201_026.mp3"
        }
    ]
}