{
    "Txt_Name": "Event_013_02",
    "Dialogue": [
        {
            "index": 1,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "……あ！",
            "voice": "event/Event_013/Event_013_02/event_013_02_001.mp3"
        },
        {
            "index": 2,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "何？　どうしたの？　そんな大声出して。",
            "voice": "event/Event_013/Event_013_02/event_013_02_002.mp3"
        },
        {
            "index": 3,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "残念。まほろの目玉焼き作る分の卵がないよ。",
            "voice": "event/Event_013/Event_013_02/event_013_02_003.mp3"
        },
        {
            "index": 4,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "卵1個でいいなら出来るけど。",
            "voice": "event/Event_013/Event_013_02/event_013_02_004.mp3"
        },
        {
            "index": 5,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "あんたの卵は？",
            "voice": "event/Event_013/Event_013_02/event_013_02_005.mp3"
        },
        {
            "index": 6,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "2個だよ。",
            "voice": "event/Event_013/Event_013_02/event_013_02_006.mp3"
        },
        {
            "index": 7,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "あんたが1個にしなよ。",
            "voice": "event/Event_013/Event_013_02/event_013_02_007.mp3"
        },
        {
            "index": 8,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "おっと。なかなか横暴なことを言ってくれますねえ。",
            "voice": "event/Event_013/Event_013_02/event_013_02_008.mp3"
        },
        {
            "index": 9,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "自覚はしてる。",
            "voice": "event/Event_013/Event_013_02/event_013_02_009.mp3"
        },
        {
            "index": 10,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あははは。\nでも、んー……、そうだねえ……。じゃあ……。",
            "voice": "event/Event_013/Event_013_02/event_013_02_010.mp3"
        },
        {
            "index": 11,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "うん。目玉焼きだとキレイに2人で分けらんないから、\n卵3個でオムレツにして、それを2人で分けよっか。",
            "voice": "event/Event_013/Event_013_02/event_013_02_011.mp3"
        },
        {
            "index": 12,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "どう？\nそれなら卵3個でも平和に2人で朝ご飯に出来るんじゃない？",
            "voice": "event/Event_013/Event_013_02/event_013_02_012.mp3"
        },
        {
            "index": 13,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "いいじゃん。そうしよう。",
            "voice": "event/Event_013/Event_013_02/event_013_02_013.mp3"
        },
        {
            "index": 14,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "ま、焼くのはあたしなんだけどねー。",
            "voice": "event/Event_013/Event_013_02/event_013_02_014.mp3"
        },
        {
            "index": 15,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "感謝してるって。",
            "voice": "event/Event_013/Event_013_02/event_013_02_015.mp3"
        },
        {
            "index": 16,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あはは。はいはい。",
            "voice": "event/Event_013/Event_013_02/event_013_02_016.mp3"
        },
        {
            "index": 17,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "ん？　絢からだ。\nごめんまほろ、代わりに出てくれる？",
            "voice": "event/Event_013/Event_013_02/event_013_02_017.mp3"
        },
        {
            "index": 18,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "ええ？　まほろコーヒー飲んでるんだけど。",
            "voice": "event/Event_013/Event_013_02/event_013_02_018.mp3"
        },
        {
            "index": 19,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "へえ。あたしはオムレツ作ってるんだけど。",
            "voice": "event/Event_013/Event_013_02/event_013_02_019.mp3"
        },
        {
            "index": 20,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "はいはい、わかってますよ……。",
            "voice": "event/Event_013/Event_013_02/event_013_02_020.mp3"
        },
        {
            "index": 21,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "もしもし？",
            "voice": "event/Event_013/Event_013_02/event_013_02_021.mp3"
        },
        {
            "index": 22,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "……うん、……うん。\nごめん、お願い。",
            "voice": "event/Event_013/Event_013_02/event_013_02_022.mp3"
        },
        {
            "index": 23,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ううん。まだ来てないよ。\nそう、美晴と一緒。",
            "voice": "event/Event_013/Event_013_02/event_013_02_023.mp3"
        },
        {
            "index": 24,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "マネージャー？　うん、まだこっちにいるけど、\nもうそろそろ出て会場でディレクターと打ち合わせしておくって。",
            "voice": "event/Event_013/Event_013_02/event_013_02_024.mp3"
        },
        {
            "index": 25,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "うん。じゃあ、よろしくね。",
            "voice": "event/Event_013/Event_013_02/event_013_02_025.mp3"
        },
        {
            "index": 26,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "電話、まほろが出たの？",
            "voice": "event/Event_013/Event_013_02/event_013_02_026.mp3"
        },
        {
            "index": 27,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "え？　なんで分かるの……。",
            "voice": "event/Event_013/Event_013_02/event_013_02_027.mp3"
        },
        {
            "index": 28,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "莉子、いま朝ご飯作ってるって。\nそれで代わりにまほろが出たんだけど……。",
            "voice": "event/Event_013/Event_013_02/event_013_02_028.mp3"
        },
        {
            "index": 29,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "ふふ。だから口調がまほろと話す時の口調だったのね。",
            "voice": "event/Event_013/Event_013_02/event_013_02_029.mp3"
        },
        {
            "index": 30,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "私、まほろと莉子で口調違うかな？",
            "voice": "event/Event_013/Event_013_02/event_013_02_030.mp3"
        },
        {
            "index": 31,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "少し声のトーンとかが、かな。\nあ、勿論、どっちのほうがどうって話じゃないわよ？",
            "voice": "event/Event_013/Event_013_02/event_013_02_031.mp3"
        },
        {
            "index": 32,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "すごいね美晴。そういうのわかるんだ。",
            "voice": "event/Event_013/Event_013_02/event_013_02_032.mp3"
        },
        {
            "index": 33,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "じゃあ、そろそろ自分は先にイベント会場向かうから。\n今日の香盤や段取りとか、ディレクターと詰めておくね。",
            "voice": ""
        },
        {
            "index": 34,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "はい。よろしくお願いします。",
            "voice": "event/Event_013/Event_013_02/event_013_02_034.mp3"
        },
        {
            "index": 35,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "あと、メイクさんなんだけど4人のメイク終わったら、\n利恵たちのいる撮影スタジオに行く手はずになってるから。",
            "voice": ""
        },
        {
            "index": 36,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "一応確認しといてもらえるかな。",
            "voice": ""
        },
        {
            "index": 37,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "はーい、わかりました。",
            "voice": "event/Event_013/Event_013_02/event_013_02_037.mp3"
        },
        {
            "index": 38,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "13時にお台場の特設イベント会場だからよろしくね。\nあ、今日は莉子の車で来るんだっけ？　",
            "voice": ""
        },
        {
            "index": 39,
            "heroineId": [
                0
            ],
            "name": "マネージャー",
            "message": "良かったね、美晴。",
            "voice": ""
        },
        {
            "index": 40,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うふふ。ありがとうございます。",
            "voice": "event/Event_013/Event_013_02/event_013_02_040.mp3"
        },
        {
            "index": 41,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ん？　どうしたの美晴？",
            "voice": "event/Event_013/Event_013_02/event_013_02_041.mp3"
        },
        {
            "index": 42,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "ううん。なんでもないの。\nそれより楽しみね。今日のイベント。",
            "voice": "event/Event_013/Event_013_02/event_013_02_042.mp3"
        },
        {
            "index": 43,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "うん。ラジオの特別公開収録。\nゲストで呼んでもらえるなんて。",
            "voice": "event/Event_013/Event_013_02/event_013_02_043.mp3"
        },
        {
            "index": 44,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "莉子なんて、すごく喜んでたもの。",
            "voice": "event/Event_013/Event_013_02/event_013_02_044.mp3"
        },
        {
            "index": 45,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "やっぱり莉子、ラジオは特別なんだね。",
            "voice": "event/Event_013/Event_013_02/event_013_02_045.mp3"
        },
        {
            "index": 46,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "うふふ。だから今日は思いっきり楽しく仕事場に向かいましょ。\n莉子の主役とラジオ出演を祝って。",
            "voice": "event/Event_013/Event_013_02/event_013_02_046.mp3"
        },
        {
            "index": 47,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ふふ。でも、それで莉子が車を用意して運転するの、\nなんだか面白いね。",
            "voice": "event/Event_013/Event_013_02/event_013_02_047.mp3"
        },
        {
            "index": 48,
            "heroineId": [
                9
            ],
            "name": "夜峰美晴",
            "message": "あら、ダメだったかしら？",
            "voice": "event/Event_013/Event_013_02/event_013_02_048.mp3"
        },
        {
            "index": 49,
            "heroineId": [
                10
            ],
            "name": "神室絢",
            "message": "ううん。莉子、嬉しそうだったし。",
            "voice": "event/Event_013/Event_013_02/event_013_02_049.mp3"
        },
        {
            "index": 50,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "絢、なんだって？",
            "voice": "event/Event_013/Event_013_02/event_013_02_050.mp3"
        },
        {
            "index": 51,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "財布忘れたんだってさ。",
            "voice": "event/Event_013/Event_013_02/event_013_02_051.mp3"
        },
        {
            "index": 52,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "あはは。それは焦るだろうねえ。\nあたしたちも忘れないようにしないと。",
            "voice": "event/Event_013/Event_013_02/event_013_02_052.mp3"
        },
        {
            "index": 53,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "しかし、なんで財布を忘れるかね。",
            "voice": "event/Event_013/Event_013_02/event_013_02_053.mp3"
        },
        {
            "index": 54,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "さっき寮を出る時、慌てて準備してたっぽかったからね。",
            "voice": "event/Event_013/Event_013_02/event_013_02_054.mp3"
        },
        {
            "index": 55,
            "heroineId": [
                12
            ],
            "name": "日名倉莉子",
            "message": "ほい、お待たせ。オムレツ焼けたよー。\nまほろ、ちょっとお皿取ってくんない？",
            "voice": "event/Event_013/Event_013_02/event_013_02_055.mp3"
        },
        {
            "index": 56,
            "heroineId": [
                11
            ],
            "name": "宮路まほろ",
            "message": "おっけー。お、おいしそう。\nうん。目玉焼きよりこっちのほうで正解かも。",
            "voice": "event/Event_013/Event_013_02/event_013_02_056.mp3"
        }
    ]
}